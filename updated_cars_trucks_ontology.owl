<owl:NamedIndividual rdf:about="#Step_52632">
  <rdf:type rdf:resource="#Step"/>
  <has_image rdf:resource="#StepImage_52632_GhD4LSi4BjFFk6Z4.standard"/>
  <has_image rdf:resource="#StepImage_52632_mRpfHRstjOGx4GJV.standard"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This is what the front panel looks like, for the US model. The unit is manfactured by Clarion for Subaru. There is a China-specific model with different front panel buttons. The top of the case shows the Clarion part number, PF-3391A-A. A web search for this part number will find the factory service manual available, although not from Clarion.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_52632_GhD4LSi4BjFFk6Z4.standard">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/GhD4LSi4BjFFk6Z4.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_52632_mRpfHRstjOGx4GJV.standard">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/mRpfHRstjOGx4GJV.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_52633">
  <rdf:type rdf:resource="#Step"/>
  <has_image rdf:resource="#StepImage_52633_rrGnJayaJ3E5dwME.standard"/>
  <has_image rdf:resource="#StepImage_52633_VOsmEx6eqLSSkbVq.standard"/>
  <has_image rdf:resource="#StepImage_52633_VoEQRQCDU2SVMNiR.standard"/>
  <requiresTool rdf:resource="#screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the top of the case by removing the single screw at the rear; then use a small screwdriver to release the top cover from the case, starting at a rear corner. Lift off the cover, which has tabs at the front. This exposes the CD drive.</stepText>
</owl:NamedIndividual>

