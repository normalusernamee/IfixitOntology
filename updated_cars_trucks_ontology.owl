  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/JFwaJOIwpBEUsOAU.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_191415">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_191415"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">There is a cover protecting the Accessory Belt There are two 10mm bolts holding this splash guard on, one in the front and one in the back</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_191415">
  <rdf:type rdf:resource="#StepImage"/>
