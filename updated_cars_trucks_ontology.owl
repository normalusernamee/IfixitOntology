</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_54068">
  <rdf:type rdf:resource="#Step"/>
  <has_image rdf:resource="#StepImage_54068_FDoRljIlrrjJbydp.standard"/>
  <has_image rdf:resource="#StepImage_54068_aPNydTHW14wALHrE.standard"/>
