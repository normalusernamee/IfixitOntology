</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_33126">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_33126"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Install your new wheels over the lugs of the wheel adapters and tighten the lug nuts to 85 ft-lbs.</stepText>
</owl:NamedIndividual>

