  <has_model rdf:resource="#Ford_F-Series"/>
  <has_Item rdf:resource="#2004-2008_Ford_F-150"/>
  <ProcedureForThis rdf:resource="#2004-2008_Ford_F-150"/>
  <has_step rdf:resource="#Step_156443"/>
  <has_step rdf:resource="#Step_156445"/>
  <has_step rdf:resource="#Step_156446"/>
  <has_step rdf:resource="#Step_156447"/>
  <has_step rdf:resource="#Step_156448"/>
  <has_step rdf:resource="#Step_156449"/>
  <has_step rdf:resource="#Step_156450"/>
  <has_step rdf:resource="#Step_156451"/>
  <has_step rdf:resource="#Step_156452"/>
  <has_step rdf:resource="#Step_156453"/>
  <has_guideID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">78306</has_guideID>
  <guideTitle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">2006 Ford F-150 Door Handle Cable Replacement</guideTitle>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#2004-2008_Ford_F-150">
  <rdf:type rdf:resource="#Item"/>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_156443">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_156443"/>
  <requiresTool rdf:resource="#screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the plastic cover from the door handle using a 7.6 cm slotted screwdriver. Insert the screwdriver into the small opening found at the top of the plastic cover. Carefully tilt the screwdriver upward to remove the plastic cover.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_156443">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/fTEhwKagl4k1ABwm.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/S5TxfRCqlq4D3I5S.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/uCai1VeroOUNtF1p.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_156445">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_156445"/>
  <requiresTool rdf:resource="#screwdriver"/>
  <requiresTool rdf:resource="#6_allen_wrench"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the two 6 mm hex screws using a 6mm socket. Turn the screwdriver counter-clockwise to loosen and remove the screws. Grab the door handle and remove it from its location.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_156445">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/UWqZhbJcsL6cpgMK.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/4VZkPxHuBTBmpEnV.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/HuMcA1hTY1HIlTVs.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_156446">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_156446"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the top plastic cover by pulling outward to snap it out of its location. There are not any screws to remove in this part.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_156446">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/ZtlkZjvZetamBUlI.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/vMXhUTDiJGLVEC3X.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/VGOxAqoygSZ2BsSO.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_156447">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_156447"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the other plastic cover by pulling outward to snap it out of its location. There are not any screws to remove in this part.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_156447">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/cDriW1L3alneQlKS.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/5wokLTPWponLIp3k.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_156448">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_156448"/>
  <requiresTool rdf:resource="#6_allen_wrench"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the three 6 mm hex screws off the main door cover using a 6 mm socket. There are two screws at the top and one  at the bottom. The panel itself should stay in place even after the screws have been removed. The door panel is maintained in place by plastic hooks behind it.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_156448">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/dIIqYnQXwdM1e65D.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/FLWLIgmrXGGdKGpL.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/AY5vsLGvfs4cguNc.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_156449">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_156449"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Gently, but with a strong force, lift the door cover about an inch or two high. Then, carefully pull it outward. Be careful not to damage the plastic hooks that are keeping the door in place.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_156449">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/okBwYHFCKcqKQvCH.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/eA1B1dGreZRZHtAL.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/6uaKhgekQU34CYVx.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_156450">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_156450"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Snap out the upper end of the cable out of its location. The upper end of the cable has a shape of a little ball. The little ball is found by where the door handle was removed. Using a plier, lift the cable high enough so that the little ball can come out through the round opening above it.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_156450">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/RFUZMlvAdtbGyw3J.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/xsMf5ddjKKqkBt4v.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/NRRPeilbcDi3OXNF.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_156451">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_156451"/>
  <requiresTool rdf:resource="#screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the three T27 hex star screws using a tamper torx screw driver. The screws are found at the bottom of the door. You may need a mechanic creeper to lay under the door. Wear protective goggles if you lay down under the door to do this step.</stepText>
</owl:NamedIndividual>

