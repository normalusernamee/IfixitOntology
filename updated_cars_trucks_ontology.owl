</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109685">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109685"/>
  <requiresTool rdf:resource="#phillips_screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use your Philips Head screwdriver to unscrew the farthest #6 3/4" Stainless Philips Screw. Be careful not to loose the rubber gasket attached to the screw.</stepText>
</owl:NamedIndividual>

