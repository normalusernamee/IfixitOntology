  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Attach stroboscopic timing light's current sensor to the wire leading to Cylinder #1 You can identify the cylinder by the printed number. The current sensor attaches around the wire leading out of the cylinder.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_42322">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/b5vu2XSvFvZEj5EU.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_40416">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_40416"/>
  <requiresTool rdf:resource="#9_16_wrench"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using a 9/16 inch wrench, loosen the distributor adjustment bolt and leave the wrench in place</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_40416">
  <rdf:type rdf:resource="#StepImage"/>
