</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_42985">
  <rdf:type rdf:resource="#Step"/>
  <has_image rdf:resource="#StepImage_42985_LYRENOCgqqgbwngi.standard"/>
  <has_image rdf:resource="#StepImage_42985_lZ2CHhUSLw5NRot1.standard"/>
