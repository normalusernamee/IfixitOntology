  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_18356"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">16</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Apply a small amount of dielectric grease to the metal contacts to prevent corrosion.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_18356">
