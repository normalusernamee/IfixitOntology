  <requiresTool rdf:resource="#jack"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Place jack under jack plate, under appropriate tire. Jack plates are welded to frame with two tabs and raised dots to indicate index with jack.</stepText>
</owl:NamedIndividual>

