  <requiresTool rdf:resource="#torx_socket_set"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">There are 2 snaps and one screw holding on the splash shield The top two are a Phillips #2 located directly behind the tire The one attached to the bumper is a Torx Pull back the splash guard for better visibility</stepText>
</owl:NamedIndividual>

