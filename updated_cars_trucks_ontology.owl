</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_17007">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_17007"/>
  <requiresTool rdf:resource="#phillips_2_screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the four Phillips screws that hold the speaker in the door. Lift the speaker out of the door. It is still connected by a single connector. Press down on the tabs on the speaker connector, and pull it directly away from the speaker.</stepText>
</owl:NamedIndividual>

