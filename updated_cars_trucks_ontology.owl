</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_165173">
  <rdf:type rdf:resource="#Step"/>
  <has_image rdf:resource="#StepImage_165173_E11CtKLU2p3kdw1D.standard"/>
