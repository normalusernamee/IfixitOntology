</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_18350">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_18350"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">13</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Cover the electrical contacts with a shop towel to prevent throttle body cleaner from inadvertently spilling over.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_18350">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/UDyYWuwTNIwyofwn.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/LaRmXoIJYFPtkHVm.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_18352">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_18352"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">14</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Liberally apply throttle body cleaner to the inside of throttle body and butterfly valve. Use a shop towel sprayed with cleaner to wipe off thick grime if severely dirtied.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_18352">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/HhbWQONfbWagAZXj.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_18355">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_18355"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">15</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Spray electrical cleaner on the metal contacts to blast away particles in the bracket. Spray off the wiring clip that attaches to the throttle body inside the engine bay.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_18355">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/SdTRgXEunisNASJB.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/kgfuj5ag3LZtJFZq.standard</toolImageURL>
</owl:NamedIndividual>

