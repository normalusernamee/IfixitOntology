</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#21_mm_socket">
  <rdf:type rdf:resource="#Tool"/>
  <toolURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">/Item/21_mm_Socket</toolURL>
</owl:NamedIndividual>

