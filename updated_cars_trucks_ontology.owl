  <has_image_for_step rdf:resource="#StepImage_109524"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Place the cross-bar back in place. When placing the cross-bar back in, make sure the bars are properly hooked into place.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109524">
