  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">WIth the 13mm wrench, remove the red cable.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_46119">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/faXHSTGsrWoLSFWW.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_46120">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_46120"/>
  <requiresTool rdf:resource="#13_wrench"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Unscrew the bottom screw  identify in step 2 with the 13mm socket wrench. Remove the old battery.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_46120">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/KmQCpD3YY1WpKMOV.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_46121">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_46121"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Insert your new battery and lock the it with the screw. Grease the battery contacts.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_46121">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/h22CwKsoRbUUxSu2.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_46122">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_46122"/>
