<owl:NamedIndividual rdf:about="#Step_40884">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_40884"/>
  <requiresTool rdf:resource="#5_8_spark_plug_socket"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use the 1/2" socket to loosen and remove the four bolts and washers on the spark plug cover labeled 2.4L Twin Cam. These four bolts should be torqued to 18 ft-lbs when putting the spark plug cover back on.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_40884">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/N5BcpW15SAxMBUI6.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/MpFUXXNNrVsQJP3Y.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/XDyOE5OjdlZANq1b.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_40885">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_40885"/>
  <requiresTool rdf:resource="#flathead_screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">With a flathead screwdriver remove the ignition coil electrical connector on the right side of spark plug cover. Be careful while disconnecting the ignition coil electrical connector as it may break.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_40885">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/JvYDSZQrh1RKVQKM.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/42KfXdUuZw3Eabgt.standard</toolImageURL>
