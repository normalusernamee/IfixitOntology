<owl:NamedIndividual rdf:about="#Step_38344">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_38344"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">2008 Toyota Sienna airfilter with broken plastic vacuum connector thanks to a reckless oil change facility.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_38344">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/3vYNN1lLloVbSg5R.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_38345">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_38345"/>
  <requiresTool rdf:resource="#drill"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove cover for the vehicle and carefully saw off the rest of the broken connector so it is flush with the cover and will be easier to drill out.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_38345">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/61SVlsLGJ1I4lIEa.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_38346">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_38346"/>
  <requiresTool rdf:resource="#drill"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Drill out the old remaining connector with a 9/32 inch drill bit. ** you must use a 9/32 only so that the new metal connector with fit snug.**</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_38346">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/cTDnaA1DTny3JNj1.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_38347">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_38347"/>
  <requiresTool rdf:resource="#loctite_glue"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Insert new metal connector with a little loctite glue using a small box wrench.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_38347">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/w1MpAkhNFxkUNWYm.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/ELeMDt1XeFJiWwaM.standard</toolImageURL>
