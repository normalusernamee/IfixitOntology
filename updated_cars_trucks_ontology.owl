  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Carefully pull the handle housing off. Do not pull too hard. There are wires attached to the back of the housing.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_128965">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/flbBSWsxTeMVZGWh.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/yVmBYH6ykUB6KGUY.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_128966">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_128966"/>
  <requiresTool rdf:resource="#flathead_screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Detach the wires on the handle housing with a flathead screwdriver by pushing upwards on the release slot.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_128966">
