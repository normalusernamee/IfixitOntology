  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/3TG33CZRAgbuLCdY.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_54075">
  <rdf:type rdf:resource="#Step"/>
  <has_image rdf:resource="#StepImage_54075_QbQk4YgGWblNIGq1.standard"/>
  <has_image rdf:resource="#StepImage_54075_jgEMdE6BkMwq2u2O.standard"/>
  <requiresTool rdf:resource="#t9_torx_screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">17</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove four 6 mm T9 Torx screws. Carefully remove the metal plate, guiding the two wires through the holes.</stepText>
</owl:NamedIndividual>

