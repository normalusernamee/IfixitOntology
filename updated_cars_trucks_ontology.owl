  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/FPHDvqknD3Rhapat.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_166617">
  <rdf:type rdf:resource="#Step"/>
  <has_image rdf:resource="#StepImage_166617_KVoMHDPIJyYAT6mq.standard"/>
  <has_image rdf:resource="#StepImage_166617_Enobrv2QiVhqFWj3.standard"/>
  <has_image rdf:resource="#StepImage_166617_EVENIyo1PiwgpsTY.standard"/>
