<owl:NamedIndividual rdf:about="#Step_109238">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109238"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate the two plastic screw covers that are adjacent to the rear light covers.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109238">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/fVBUumwQDP4tkrsk.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109240">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109240"/>
  <requiresTool rdf:resource="#flathead_screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using a flathead screwdriver, pry out both plastic screw covers.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109240">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/KvSljpRZNCrSS6AQ.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109242">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109242"/>
  <requiresTool rdf:resource="#8_mm_deep_socket_wrench"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Once covers are removed, there will be an 8 mm hex head screw located beneath each cover.</stepText>
</owl:NamedIndividual>

