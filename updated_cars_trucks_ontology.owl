<owl:NamedIndividual rdf:about="#Step_27532">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_27532"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Find the hood release lever located at the bottom left the driver's console. Pull the lever until you hear the hood click open. Lift the hood slightly and locate the hood latch. The latch will be near the center of the hood. Pull up on the hood latch and lift the hood open. Place the hood prop rod into the hole located at the front of the hood on the passenger side.</stepText>
</owl:NamedIndividual>

