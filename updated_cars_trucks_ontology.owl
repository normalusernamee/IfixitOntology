</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Chevrolet_Corvair">
  <rdf:type rdf:resource="#Model"/>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#1965-1969_Chevrolet_Corvair">
