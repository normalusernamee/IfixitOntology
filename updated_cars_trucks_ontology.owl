</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109495">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109495"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the battery by lifting it straight up and out. Avoid tilting the battery in order to prevent an acid spill.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109495">
