<owl:NamedIndividual rdf:about="#Step_40682">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_40682"/>
  <requiresTool rdf:resource="#10_mm_hex_driver_socket"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a 10 mm hex driver or socket to remove the four screws that hold the spark plug cover in place. Lift the plastic cover and set it aside.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_40682">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/HqTplX1RLfTVLMGF.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/Lnn2pAQBTpti5UpT.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_40074">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_40074"/>
  <requiresTool rdf:resource="#5_8_spark_plug_socket"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the spark plug wire by pinching the locking tabs on either side of it and pulling it out.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_40074">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/SvxYbHi2slX2VKVB.standard</toolImageURL>
