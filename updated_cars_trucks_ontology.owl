<owl:NamedIndividual rdf:about="#socket_8_mm">
  <rdf:type rdf:resource="#Tool"/>
  <toolURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.amazon.com/TEKTON-14140-Shallow-Socket-6-Point/dp/B000NY8PVE/</toolURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_31097">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_31097"/>
  <requiresTool rdf:resource="#socket_wrench"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate and remove the bolts in the front of the wheel well on the either side of the car using a #1 Phillips screw driver or an 8 mm socket wrench. Locate the bolts on the bottom of both corners of the bumper and remove them using an 8 mm socket wrench or box end wrench.</stepText>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</stepOrder>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_31097">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/Re4HVIwxuupvFyHh.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/EeOHENIiid3eTn2e.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_31098">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_31098"/>
  <requiresTool rdf:resource="#flathead_screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate the plastic push pins along the bottom of the bumper. Use a small thin prying device, such as  a flathead screwdriver, to pop the heads of the push pin up. Then remove the entire push pin assembly from their holes.</stepText>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</stepOrder>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_31098">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/OLMmpxVQvvf5nxKV.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/KAEYpmSZLYS3KAcC.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/QAVfJkm6XSLNaVAX.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_31099">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_31099"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the push pins on the two raised portions of the bumper next to the inside of the headlights. There are four clips along the top of the bumper still holding it in place. Gently lift the bumper while pulling it away from the car to free the clips and remove the bumper.</stepText>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</stepOrder>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_31099">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/Rjt1LXESuYe4qbIc.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/YX2wtu2oZIhiJnaI.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/WMwPtQXGDXLwwSaO.standard</toolImageURL>
</owl:NamedIndividual>

