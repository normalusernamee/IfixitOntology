<owl:NamedIndividual rdf:about="#Step_15102">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_15102"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The brake fluid reservoir is located towards the back of the engine compartment. Unscrew the cap and remove it. Be careful when handling brake fluid. Wash hands immediately if they come in contact with it. Do not let the fluid touch any painted parts; it will strip the paint.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_15102">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/2gl1hqOGNPCHBtdv.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/bNZDQnOZniNKe5mU.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_15103">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_15103"/>
  <requiresTool rdf:resource="#turkey_baster"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a turkey baster to remove 2-3 ounces of brake fluid from the reservoir. Never let the fluid level drop below the minimum mark. This will let air into your lines, which will require bleeding to remove. Properly dispose of old brake fluid. Do not reuse it.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_15103">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/RQSCEaSu1WZNCkQq.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_15104">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_15104"/>
  <requiresTool rdf:resource="#flathead_screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a flat head screwdriver or brake spoon to remove the outer pad retaining clip. The clip has a lot of spring energy. Hold onto it with a pair of pliers when removing it to keep it from flying off and injuring someone.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_15104">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/T4csjtVsMXiW4xLC.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_15105">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_15105"/>
  <requiresTool rdf:resource="#flathead_screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a flat head screwdriver to remove the black plastic guide bolt dust caps.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_15105">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/KxFInEyBOKb6qTXu.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_15106">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_15106"/>
  <requiresTool rdf:resource="#7_allen_wrench"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a 7 mm Allen wrench to remove the two caliper guide bolts.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_15106">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/jBwALI4OwYCvGWwF.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_15107">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_15107"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the clip that holds the brake line to the strut with a pair of vise grips. Pull the brake line towards you and out of the bracket.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_15107">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/WqXmD1LEGDPHJfGX.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/dnFBlW5i1xICxLXy.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_15108">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_15108"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">11</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull the caliper off of the rotor. Place the caliper on top of a box or bucket so the brake line doesn't stretch or kink.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_15108">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/2KGjj6u4YQJaXoiI.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/nkSbDpJEMnIwEiS5.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_15109">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_15109"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">12</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull the outer brake pad from the mounting bracket.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_15109">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/ytj2SxFFa5cKfyjs.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_15110">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_15110"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">13</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the inner brake pad from the caliper. The pad is held in place by three metal clips. It may take some force and wiggling to free the pad.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_15110">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/fATIhgxRkPqtylOK.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_15111">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_15111"/>
  <requiresTool rdf:resource="#flathead_screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">14</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This step is only necessary if you are replacing the guide bolt bushings. Use a flat head screwdriver to push the bushing through its hole in the caliper. Remove the bushing from the caliper.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_15111">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/hQ1cAVamJOGdklZJ.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/yJATsB1nChncYK5V.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_15112">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_15112"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">15</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the two 15 mm mounting bracket bolts. Lift the mounting bracket off the rotor.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_15112">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/YHVNIFVCTYKcLCkJ.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/CUxDpCOIuoDt6eRY.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_15113">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_15113"/>
  <requiresTool rdf:resource="#lug_wrench"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">16</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the brake rotor by pulling it straight off the lugs. If the rotor is to be reused, it is a good idea to take it to a shop to check that it is not out of round and to have it turned.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_15113">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/MZxiPbAbwrVbovby.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_15114">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_15114"/>
  <requiresTool rdf:resource="#lug_wrench"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">17</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Install the new rotor over the wheel lugs.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_15114">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/2P1dpfMSJTpbEHbQ.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_15115">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_15115"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">18</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Reinstall the mounting bracket. Grease the mounting bracket bolts and install them. Mounting bracket bolts should be tightened to 133 ft-lbs.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_15115">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/iCDwEJbPlvEhmt3l.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_15116">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_15116"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">19</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Clean the rotor, caliper, and all other hardware with an aerosol brake cleaner. Never use compressed air to clean brake components. Brakes contain asbestos, and blowing the particles into the air could cause serious respiratory problems if inhaled.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_15116">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/2qYmqO1mtNLd2YGI.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_15117">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_15117"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">20</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Coat the steel backing plates of the new brake pads with an anti-squeal coating.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_15117">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/jQ22VYt4FbGvaCSr.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_15118">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_15118"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">21</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Place the new outer pad on the mounting bracket. Small particles are a common cause of brake noise. Make sure pad and rotor surfaces are clean of all dirt and debris before installing.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_15118">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/Xpfbwk6NeVIaNoSk.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_15119">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_15119"/>
  <requiresTool rdf:resource="#flathead_screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">22</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This step is only necessary if the guide bolt bushings were removed. Insert the new bushing from the inside of the caliper. Use a flat head screwdriver to help feed the bushing through its hole.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_15119">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/3WLMtGB1qyJKUFCv.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/YKsR2dkvj2meJ2ue.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_15120">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_15120"/>
  <requiresTool rdf:resource="#c-clamp"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">23</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Compress the caliper piston with a C-clamp. Continually check the fluid level in the brake fluid reservoir to make sure it doesn't exceed the maximum level. Install the new inner pad by pressing it into the caliper and making sure the clips are secure.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_15120">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/tvBf4JmD4RhG2Bfx.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/KagU1aYPaCu4KVKw.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_15121">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_15121"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">24</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Place the caliper back on the rotor.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_15121">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/OcuXfZPYIYDPDRop.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_15122">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_15122"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">25</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Grease the two guide bolts and reinstall them. Guide bolts should be torqued to 21 foot-pounds. Reinstall the guide bolt dust caps.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_15122">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/DSCmJ234fQnVdZWH.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/YsPgfGh32FLiUVvs.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_15123">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_15123"/>
  <requiresTool rdf:resource="#wrench"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">26</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Place the brake line back in the bracket on the strut. Secure the brake line with the retaining clip. Use the blunt end of a wrench to knock it completely into place.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_15123">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/5Xs1SwxukTlnIWVv.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_15124">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_15124"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">27</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Reinstall the retainer clip by first inserting the ends into the two holes in the caliper, then use pliers to stretch the ends onto the mounting bracket. It is always a good idea to use white lithium grease wherever you have metal-on-metal contact.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_15124">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/KdTj2xfWS2AJVWFr.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_15125">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_15125"/>
  <requiresTool rdf:resource="#lug_wrench"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">28</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Put the wheel back on the car and hand tighten the lug nuts. Lower the car and completely tighten the lug nuts Lug nuts should torqued to 94 foot-pounds.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_15125">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/XpEbfKRe56YSKpo4.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_15126">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_15126"/>
  <requiresTool rdf:resource="#hydraulic_floor_jack"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">29</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Place the cap back on the brake fluid reservoir. Turn the key to the "on" position. Press the brake pedal a few times. It should go to the floor once, and then become firm. Fill the brake fluid reservoir with new fluid, if needed.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_15126">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/XU2RNHlVXviIaniI.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/aNMVXYpCvJVbI3VF.standard</toolImageURL>
</owl:NamedIndividual>

