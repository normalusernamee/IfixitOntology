  <has_step rdf:resource="#Step_56572"/>
  <has_guideID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">20580</has_guideID>
  <guideTitle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">How to clean Mercedes W123 Electric Ground contacts</guideTitle>
</owl:NamedIndividual>

