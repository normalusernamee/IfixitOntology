  <has_step rdf:resource="#Step_97360"/>
  <has_step rdf:resource="#Step_97361"/>
  <has_step rdf:resource="#Step_97362"/>
  <has_step rdf:resource="#Step_97363"/>
  <has_step rdf:resource="#Step_97364"/>
  <has_step rdf:resource="#Step_97365"/>
  <has_guideID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">42160</has_guideID>
  <guideTitle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">2004-2008 Mazda RX-8 Tire Rotation</guideTitle>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_97359">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_97359"/>
  <requiresTool rdf:resource="#socket_wrench"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Loosen lug nuts with the socket/wrench just until they begin to turn. Removing lug nuts requires a 21mm socket</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_97359">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/WIXCOYKOfsVhSia4.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_97360">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_97360"/>
  <requiresTool rdf:resource="#hydraulic_floor_jack"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Place Jack under the car (as shown). It's best to avoid using the jack included with the car. If at all possible, use at least a 2 ton floor jack. Use the Jack Handle to crank up just enough so that the tire noticeably lifts off the ground.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_97360">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/ZCOlVGEvmIQIeCtx.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/vCuEONad4mGiVOLw.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/eqFxeEXLAUTZ1kEH.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_97361">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_97361"/>
  <requiresTool rdf:resource="#jack_stand"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Put the jack stand under the car to ensure the car will not fall back down while you are working on it.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_97361">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/KUkPYQyvYoJm5u3J.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_97362">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_97362"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove all lug nuts holding the wheel on the car.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_97362">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/JfFIKmPOrgdRRRqv.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_97363">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_97363"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the front wheel from the vehicle.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_97363">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/A1JGVThaHrLXDDol.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_97364">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_97364"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">With the front wheel off repeat step 4 and step 5 to remove the rear wheel as well.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_97364">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/H6ExRZEkmnXCbTAA.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_97365">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_97365"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Place the rear wheel on the front of the car and place the front wheel in the rear. Tighten up the lug nuts and your tire rotation is complete. Begin to tighten the lug nuts as much as you can with the wheel in the air, and then lower the car to the ground to finish tightening. Tighten the nuts in a star pattern to avoid over tightening one side leaving the wheel installed crooked Mazda recommends that the lug nuts be torqued to 108 ft*lbs to ensure that the wheel is secure</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_97365">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/oxYYA3NPFJ433Pvq.standard</toolImageURL>
</owl:NamedIndividual>

