<owl:NamedIndividual rdf:about="#Step_109409">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109409"/>
  <requiresTool rdf:resource="#oil"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Park your car on a level surface. Make sure to put the car in park (first gear for manual transmissions) with the emergency break engaged. Do not change oil within 2 hours of driving the car to allow the oil time to cool.</stepText>
</owl:NamedIndividual>

