</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_18349">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_18349"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">12</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lift the throttle body off of the intake manifold. The manifold is now out of the engine bay and ready for cleaning or adjustment.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_18349">
  <rdf:type rdf:resource="#StepImage"/>
