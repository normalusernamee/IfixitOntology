<owl:NamedIndividual rdf:about="#3_8_socket">
  <rdf:type rdf:resource="#Tool"/>
  <toolURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.amazon.com/TEKTON-14130-8-Inch-Shallow-6-Point/dp/B000NPULBK/ref=sr_1_1?s=hi&amp;ie=UTF8&amp;qid=1434651838&amp;sr=1-1&amp;keywords=tekton+14130</toolURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109148">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109148"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'''Open the trunk door. ''' In order to remove the rear light assembly, it must be accessed with the trunk open.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109148">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/xmvDWAkbSTIAQwfN.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109154">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109154"/>
  <requiresTool rdf:resource="#3_8_socket"/>
  <requiresTool rdf:resource="#socket_wrench"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'''Remove the three screws '''that are holding the rear light assembly to the body of the Jeep. Use the wrench to remove the screws. Each screw is an indented hexagon washer head and is 3/16" in diameter and 3/4" long. All three screws are identical and can be removed in any order.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109154">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/5BATf1Pj1nF2PN3F.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109161">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109161"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'''Gently pull the rear light assembly away''' from the car to gain access to the bulbs and wires. Be careful when pulling away the rear light assembly.  The wires will still be attached to the inside of the car.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109161">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/FeOakVBsOeGVPNG3.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109642">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109642"/>
  <requiresTool rdf:resource="#socket"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'''Locate the brake light. '''Turn the light assembly over and you will see three light bulb sockets.  The brake light is the top bulb and the bulb behind the red plastic lens.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109642">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/mTCD1UcYj1OL4oYt.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109646">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109646"/>
  <requiresTool rdf:resource="#socket"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'''Remove the brake light. '''Twist the light bulb socket clockwise and remove the light bulb socket from the rear light assembly.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109646">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/plcs33dE2NfkHwhQ.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/lQS4OMSNIhblqMss.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/KMtMnkDhJMoGOLRX.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109661">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109661"/>
  <requiresTool rdf:resource="#socket"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'''Remove the bulb''' by pinching on either side of the bulb and gently pulling the bulb out. Be careful not to force the bulb when pulling as it may damage the bulb socket.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109661">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/ysbAnblEjbSe2AP3.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109664">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109664"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'''Replace with the new bulb. '''Gently put in the new bulb in the same manner as you removed the old bulb.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109664">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/JsoFIDXURwy1Lq4d.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109667">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109667"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'''Lock the bulb back in place. '''Carefully put the bulb back into the light assembly and twist counter clockwise until it locks in place.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109667">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/NsYO1HssoLVOHWW1.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/icxeRpAaJulNBnAJ.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109675">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109675"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'''Set the light assembly back in place. ''' There is a tab on the bottom of the light assembly that must line up with the hole in the car. The red brake light should be on top.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109675">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/qHSdcIWBIR1sK2gT.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/lLsGHE2BFCjaggOh.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/WxUrRVsFHHLWNx3A.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109678">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109678"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'''Tighten the three screws''' back into place. This will fasten the light assembly back onto the car. The order and the positioning of the screws will have no effect on the assembly, because they are all identical.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109678">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/rmMGvtURgSlWGJNI.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109681">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109681"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">'''Check the brake light.''' Have another person step on the brake to ensure proper lighting occurs.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109681">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/iHtsn3bV4fBTxjXX.standard</toolImageURL>
</owl:NamedIndividual>

