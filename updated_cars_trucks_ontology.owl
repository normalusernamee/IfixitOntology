  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the brush assembly by pulling it out.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_132135">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/m2md3ANMUnnqZMYv.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/BHbLAT5YMCU6Flxk.standard</toolImageURL>
