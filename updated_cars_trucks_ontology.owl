</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Procedure_42200">
  <rdf:type rdf:resource="#Procedure"/>
  <has_make rdf:resource="#Scion"/>
  <has_Item rdf:resource="#2011-2013_Scion_tC"/>
  <ProcedureForThis rdf:resource="#2011-2013_Scion_tC"/>
  <has_step rdf:resource="#Step_97510"/>
  <has_step rdf:resource="#Step_97511"/>
  <has_step rdf:resource="#Step_97513"/>
  <has_step rdf:resource="#Step_97512"/>
  <has_step rdf:resource="#Step_97514"/>
  <has_guideID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">42200</has_guideID>
  <guideTitle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">2011-2013 Scion tC Car Battery Replacement</guideTitle>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_97510">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_97510"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Begin by opening the hood of the car and locating the battery on the front of the driver side in the engine compartment</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_97510">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/NVNiwCgGlDEbAYrH.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_97511">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_97511"/>
  <requiresTool rdf:resource="#10_mm"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using a socket wrench, loosen the 2 (5/16 inch) nuts on the top of the battery support bracket Remove the bracket by lifting up</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_97511">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/HQjvfihEqlGDJbGA.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_97513">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_97513"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">After removing the bracket, locate the supporting rod behind the battery The rod is hooked on  a hole in the metal behind the battery. Unhook the rod and remove it</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_97513">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/xbf13PCDAUGRfpVM.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_97512">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_97512"/>
  <requiresTool rdf:resource="#10_mm_socket"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Start with the negative terminal of the battery Loosen the nut on the negative terminal with a 10mm socket wrench Remove the wire by slightly twisting and pulling up</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_97512">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/5K3uVqGWUUycJ6iY.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/DBesAjJkYXpWmWwG.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_97514">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_97514"/>
  <requiresTool rdf:resource="#10_mm_socket"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the cap from the positive terminal of the battery Using a 10mm socket wrench, loosen the nut on the positive terminal Remove the wire from the positive terminal by slightly twisting and pulling up Remove the battery by lifting it out of the engine compartment</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_97514">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/etK3CfKWruBKmHsQ.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/ffdstWODTAE36soD.standard</toolImageURL>
</owl:NamedIndividual>

