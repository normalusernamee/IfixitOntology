  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/BZHsjbGKEsLBmq43.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Procedure_21518">
  <rdf:type rdf:resource="#Procedure"/>
