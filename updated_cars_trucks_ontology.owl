</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_54080">
  <rdf:type rdf:resource="#Step"/>
  <has_image rdf:resource="#StepImage_54080_koHGL26AEEQOdHnD.standard"/>
  <has_image rdf:resource="#StepImage_54080_AwiyBYmnGDDlSRu1.standard"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">19</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">You can now remove the rotary push button unit from the back. Repeat these same procedure for the rotary push button on the right side.</stepText>
</owl:NamedIndividual>

