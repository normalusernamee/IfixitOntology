  <has_guideID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">50410</has_guideID>
  <guideTitle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">How to Replace the Wiper Arm(s) of a 2005 Toyota Corolla</guideTitle>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#2000-2007_Toyota_Corolla">
  <rdf:type rdf:resource="#Item"/>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109097">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109097"/>
  <requiresTool rdf:resource="#flathead_screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Take the tip of your flathead screwdriver, and wedge it underneath the wiper cap. Firmly pry the wiper cap ++upwards++ until it's completely off.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109097">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/XqTCIdlWHN5HmwSH.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/YiG2dbDShbQShgfD.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109103">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109103"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Place the head of your ratchet on the exposed nut. Loosen the nut by turning your ratchet counter clockwise.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109103">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/bctnH5vLabDlHlKQ.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/Kfl3peSjgBFyCEZI.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109108">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109108"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Once the nut is loose enough, you may grab it and place it to the side.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109108">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/WSOoCKGgBwJEHNWK.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/aTcBQbJTMQ3Ta5n5.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109110">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109110"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Firmly grip the lower end of the wiper arm. Pull it towards you, so that the wiper arm is extended in the upward position.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109110">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/L2IGwqUEepJYXvuT.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/NAHNDkTWUyOhQeG4.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109114">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109114"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">While holding the lower end of the wiper arm, tug the wiper arm upwards until completely detached. Don't be discouraged if the wiper arm isn't detached right away. You may have to tug upwards a few times to detach it.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109114">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/vAxRfUfFCFKVdRlu.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/ZeyGNJ52e5DJFqPr.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109118">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109118"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Have your new wiper arm extended in the upward position (just how your old wiper arm was extended at the end of Step 4). Take your wiper arm, and center its hole on the exposed metal threading.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109118">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/kwNN2qh16A1IeVTS.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/2vSlTKZkBXtU5vw3.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109119">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109119"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Once the wiper arm has been placed onto the metal threading, firmly grip the lower end of the wiper arm. Slowly push the wiper arm downwards and away from you, so that it's flat with the windshield. Make sure to do this slowly so that your fingers are not compressed by the wiper arm and the windshield.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109119">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/3UPi4FOiTndjoB4n.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/qpPAwE6JFxQguX6i.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109120">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109120"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Place the same nut from earlier onto the exposed threaded metal. Place the head of your ratchet onto the nut. Tighten the nut by turning your ratchet clockwise, until it can no longer be tightened.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109120">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/MyqojNoQXBb2uMpx.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/B2CVJrUNdmeuAOmG.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/o3akUqMI4AIBKRX3.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109122">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109122"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Firmly press the wiper cap upon the exposed nut. Once the wiper cap is secure, you may repeat steps 1-9 to the other wiper arm.</stepText>
</owl:NamedIndividual>

