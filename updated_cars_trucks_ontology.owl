</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_39936">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_39936"/>
  <requiresTool rdf:resource="#10_mm_hex_driver_socket"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate the hood latch beside the driver's side door, right next to the left foot rest.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_39936">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/NSjYhEA23iAKNwmh.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/diHAcUUQAKctjJHl.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_41987">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_41987"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull the latch toward you to release the hood.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_41987">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/YIuxlPgwiJUSChbd.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_39937">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_39937"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Walk around to the front of the vehicle. Slide your hand under the hood, directly above the Honda emblem. Locate and pull the hood tab to unlock the hood. Once the tab is released, lift the hood up.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_39937">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/ybu3ZsR2KjnojtMl.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/KGkLxNUjKGvWtJuj.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_39938">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_39938"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use your left hand to hold the hood up. Locate the hood stand and pull it out of the clip.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_39938">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/bk3mU1myDjWAa1p4.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_41988">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_41988"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Place the tip of the hood stand into the hole located in the upper left of the hood. Let the hood rest on the stand.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_41988">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/LnPagmD6xDNhLhXl.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_39930">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_39930"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Be sure your engine is turned off. Identify the engine's air duct hose. The emission hose is located on the top. Remove the emission hose by gently wiggling it out of place.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_39930">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/RNrmGQxeQnZJkJTJ.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/CZXQA2WLXOIngNIk.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/SREZowDsOvNwBsPS.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_40955">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_40955"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the smog hose by squeezing the tubing and pulling it off.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_40955">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/lHI5u6Xv5gSHlj16.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/O1TmC1MP1WhAIL32.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_40084">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_40084"/>
  <requiresTool rdf:resource="#phillips_2_screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using a Phillips screwdriver, loosen air duct clamp screws.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_40084">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/DnnndVyk5PhHDkQv.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_39931">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_39931"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Once the screws are loose, completely remove the air duct hose by gently wiggling it off the engine and the air filter housing.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_39931">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/JwVTiTCNUkHaSv5Z.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/6BMApRtviQXABVSy.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_39932">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_39932"/>
  <requiresTool rdf:resource="#phillips_2_screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the air filter housing cap. Using a Long Philips Screwdriver or a Long 5/16 Hex Driver, remove the four screws holding down the air filter housing cap.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_39932">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/JDrttUVAOWmICHCX.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/KkksCpQQOE2KbFg4.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/OQ6X5ixVVa4PRh6O.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_39934">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_39934"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lift the engine air filter housing cap and place it aside. Do not turn the housing cap upside down. Screws left in holes of housing cap might fall out.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_39934">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/im5ROhGbJxRGYDC5.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_41994">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_41994"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">11</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lift the dirty filter out of the housing. Vacuum any dirt or debris out of the air filter box.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_41994">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/KZscNEbhO2KvWB5B.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_39935">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_39935"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">12</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Replace the old filter with the new filter. Ensure that it seats properly in the lip of the air cleaner box. Screw down the air filter housing cap. Connect air dust, emission, and smog hoses.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_39935">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/VSGF1EZojPvarmC2.standard</toolImageURL>
</owl:NamedIndividual>

