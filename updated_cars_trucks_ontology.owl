</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_166604">
  <rdf:type rdf:resource="#Step"/>
  <has_image rdf:resource="#StepImage_166604_WsNLlilFgqypnZIB.standard"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the thin frame from the front of the device. This can be done by first unsnapping the top pegs and then the bottom ones.</stepText>
</owl:NamedIndividual>

