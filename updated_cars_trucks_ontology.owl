<owl:NamedIndividual rdf:about="#Step_167324">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_167324"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Gather the hardware, tools and protective equipment.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_167324">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/FCZmZWDDsK36vhcv.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/HLDvnKY2sAw12qE2.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_167325">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_167325"/>
  <requiresTool rdf:resource="#steering_wheel_puller_tool"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Straighten the steering wheel and remove the key from the ignition.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_167325">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/pukQmGZuFlFEon2P.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_167326">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_167326"/>
  <requiresTool rdf:resource="#steering_wheel_puller_tool"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Grip the steering wheel horn pad, placing your fingertips at the edges. Gently pull it straight towards you and the clips will release it.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_167326">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/CDNDHd6QYwfPEF51.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/31E1bRR6OMGXCieG.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/QWcTDKJ54ihUoRuD.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_167328">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_167328"/>
  <requiresTool rdf:resource="#paint_marker"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a paint marker to mark a reference point for the top center of the steering wheel on the face of the steering shaft.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_167328">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/f1OIDfaG1KKRwJBO.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_167331">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_167331"/>
  <requiresTool rdf:resource="#3_8_inch_drive_socket_ratchet_extension"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using a 19mm socket and ratchet, loosen the steering wheel retainer nut and remove it.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_167331">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/fgo2mPsGP25PEMn2.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/ASg2bWsYVxF1FJCh.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_167335">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_167335"/>
  <requiresTool rdf:resource="#steering_wheel_puller_tool"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Place the puller tool on the wheel aligning the steering shaft with the press head and the two threaded holes in the steering wheel with the holes in the ears of the puller. Run the bolts, from the puller tool, through the holes in the ears and thread them into the steering wheel hand tight.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_167335">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/UCpPhMdAHjSVAdgJ.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/uQfUDjg6fju4OPKA.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_167339">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_167339"/>
  <requiresTool rdf:resource="#steering_wheel_puller_tool"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Slowly tighten the press shaft until the steering wheel releases. This should not take much force to accomplish. If something starts bending or you have to use excessive force tightening the shaft, stop, reinstall the steering wheel retaining nut, and take the vehicle to a professional.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_167339">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/6F2SXxLMqp4yaIEr.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_167341">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_167341"/>
  <requiresTool rdf:resource="#steering_wheel_puller_tool"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Install the steering wheel hub onto the steering shaft, aligning the top center mark on the hub with the paint mark you made in Step 4.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_167341">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/rHFAVwhL5XrSdjXp.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_167343">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_167343"/>
  <requiresTool rdf:resource="#torque_wrench"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Reinstall the steering wheel retaining nut torqueing it down to 29 ft*lb using a torque wrench.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_167343">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/pNDohY4rBTVxAilR.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/qjlutxBBnBxBv4yD.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_167344">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_167344"/>
  <requiresTool rdf:resource="#steering_wheel_puller_tool"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Place the inner horn retainer ring in-between the new steering wheel and hub, aligning the six bolt holes, and place one screw, finger tight, in the hub to temporarily hold everything in place.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_167344">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/xVsdedZVvy5dMVB1.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/oRPqEZDxYCui4oiD.standard</toolImageURL>
