  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/STDwptOyRPRLiTxV.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_43073">
  <rdf:type rdf:resource="#Step"/>
  <has_image rdf:resource="#StepImage_43073_wmm2A4xJ2vKtX4ED.standard"/>
  <has_image rdf:resource="#StepImage_43073_S5S6EVLPGvY2Ki1u.standard"/>
  <requiresTool rdf:resource="#ph00_phillips_screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Flip the CD-ROM unit back over. Remove one gold colored Phillips screw located in the center.</stepText>
</owl:NamedIndividual>

