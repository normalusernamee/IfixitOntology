  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove rear brake calipers from rotors.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_18261">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/SaioQBAFLVCbPb25.standard</toolImageURL>
