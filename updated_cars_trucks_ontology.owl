</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_58887">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_58887"/>
  <requiresTool rdf:resource="#10_mm_socket"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the nut holding down the spring and plastic retainer, if present. On some cars, there is just a spring as shown in the second picture which should be removed. The nut on this car is a 10mm.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_58887">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/iYgUBlZfbX1vW6ip.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/ANYDjcRcNtFBIIFc.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_58889">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_58889"/>
  <requiresTool rdf:resource="#turkey_baster"/>
  <requiresTool rdf:resource="#drain_pan"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Now use your fluid extractor, or turkey baster (never use it in the kitchen again!) to remove enough fluid to get easy access to the filter. Dispose of the fluid in your drain pan.</stepText>
</owl:NamedIndividual>

