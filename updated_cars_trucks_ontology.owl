  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Identify the screw on the right side of the battery which maintains the battery.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_45940">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/OEYvbFAWZ4iaWsit.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_46118">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_46118"/>
  <requiresTool rdf:resource="#8_mm_box_end_wrench"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">WIth the 8mm box end wrench, remove the black (ground) cable.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_46118">
  <rdf:type rdf:resource="#StepImage"/>
