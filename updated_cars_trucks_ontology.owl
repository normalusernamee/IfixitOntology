  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/rtPjZ6KSZsmKwEKE.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/rOnQfoqeZlwbXIX2.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_93161">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_93161"/>
  <requiresTool rdf:resource="#lug_wrench"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Place spare tire onto frame. Hand tighten lug nuts (the 1, 3 and 5 positions first) until secure.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_93161">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/SYA5EFNC5boeabMV.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/M1BRsNLLjDXxxQER.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_93163">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_93163"/>
  <requiresTool rdf:resource="#lug_wrench"/>
  <requiresTool rdf:resource="#jack"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lower vehicle, turning jack counterclockwise, until tire touches ground. Tighten lug nuts (again, beginning with the 1, 3 and 5 positions) with lug wrench until secure.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_93163">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/ZHcestXCogTQ2Vto.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/NbQkJ5DiS1ePZdH3.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_93164">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_93164"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Congratulations, your tire is changed! NOTE: obey the speed limitations of your spare tire.  Exceeding the recommended speed is dangerous and can damage the spare.</stepText>
</owl:NamedIndividual>

