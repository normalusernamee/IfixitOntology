<owl:NamedIndividual rdf:about="#Step_58935">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_58935"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Jack up the front of the car so you can turn the steering wheel while the car is stationary. [guide|22770|See the guide on jacking up your car], if you need guidance on this.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_58935">
  <rdf:type rdf:resource="#StepImage"/>
