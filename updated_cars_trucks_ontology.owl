  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/CtqQkYwUNGBlQOIQ.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/XxZFRSLyDCpVbLLl.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109702">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109702"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Once the old mud flap is removed, grab your new mud flap and reattach it in the same manner as how you removed the old or broken one. Repeat for opposite front mud flap.</stepText>
</owl:NamedIndividual>

