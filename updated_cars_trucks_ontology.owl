</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#15_mm_socket_wrench">
  <rdf:type rdf:resource="#Tool"/>
  <toolURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.amazon.com/TEKTON-14147-8-Inch-Shallow-6-Point/dp/B000NPR4BA/ref=sr_1_1?ie=UTF8&amp;qid=1488405884&amp;sr=8-1&amp;keywords=15mm+socket</toolURL>
