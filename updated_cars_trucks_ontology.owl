</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_18260">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/6GyIlLPIW2vxAKZ5.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_18261">
  <rdf:type rdf:resource="#Step"/>
