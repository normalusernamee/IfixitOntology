  <requiresTool rdf:resource="#penetrating_lubricant"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Spray the area where the hard brake lines and the soft rubber hoses connect at the rear of the car with penetrating lubricant.</stepText>
</owl:NamedIndividual>

