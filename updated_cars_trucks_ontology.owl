  <has_image_for_step rdf:resource="#StepImage_128969"/>
  <requiresTool rdf:resource="#x-acto_knife"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull the insulation back to access the door handle bracket. Use an X-acto knife if needed.</stepText>
</owl:NamedIndividual>

