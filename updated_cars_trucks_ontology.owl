<owl:NamedIndividual rdf:about="#Step_120363">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_120363"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate the wiring harness bundle, which exits the firewall underneath the brake booster.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_120363">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/lIHJDDvKgDKgAbIc.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_120364">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_120364"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Identify the auxiliary wire, which is folded and taped to either the wiring harness or the fire wall.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_120364">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/luSQMdssW61sHEFg.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_120365">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_120365"/>
  <requiresTool rdf:resource="#sharp_knife"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a knife to cut the tape away from the wire and unravel it.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_120365">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/k3SNSQfm2wKTgoZP.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_120760">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_120760"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Run the wire against the battery box.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_120760">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/OOrwAS5P4YDoinOF.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_120367">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_120367"/>
  <requiresTool rdf:resource="#10_mm_wrench"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Place the ring terminal (washer end) of the wire on the right post located at the front of the battery. Place the 10 mm washer on the post. Tighten the washer using a 10 mm wrench, crescent wrench, or pliers. Post size and location may vary slightly in different models. The washer will fit snuggly around the correct post.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_120367">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/LYk1tLjfuC4BHlHn.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/Mg5aXljBR3PKJmjX.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/Ki1OURONKhimDuXX.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_120369">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_120369"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">After hooking up the wire, check to make sure the 40 amp fuse is installed for the auxiliary pin. If the fuse is not installed, there will be an empty space. This fuse can be purchased at any automotive shop.</stepText>
</owl:NamedIndividual>

