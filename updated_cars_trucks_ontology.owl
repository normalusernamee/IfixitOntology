<owl:NamedIndividual rdf:about="#Step_118536">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_118536"/>
  <requiresTool rdf:resource="#tire_plug_kit"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Wipe the surface of the tire with a rag to remove any loose dirt, rocks, or debris.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_118536">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/s5CemFTNZi1eMTKl.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_118537">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_118537"/>
  <requiresTool rdf:resource="#air_compressor"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Using the air compressor, air the tire up to the original recommended weight for the vehicle. Air is expected to continue to leak out so do not worry if you cannot get it perfect.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_118537">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/CZCxcBM1pumooEyk.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_118538">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_118538"/>
  <requiresTool rdf:resource="#air_pressure_gague"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use the air pressure gauge to make sure the tire is now aired up to its original pressure.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_118538">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/ctGM1G4sQgHNUrf1.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/XHQBGJgdCoMOcYV2.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_118539">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_118539"/>
  <requiresTool rdf:resource="#soapy_water"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Spray the surface of the tire completely with the soapy water.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_118539">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/AkUTxmEUJuNiNBK3.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_118540">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_118540"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Look for tiny air bubbles forming from the leaking air and soap combination, this is where the hole is located.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_118540">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/MuLOP4udjUVPZWxV.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_118541">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_118541"/>
  <requiresTool rdf:resource="#tire_plug_kit"/>
  <requiresTool rdf:resource="#basic_pliers"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Once the hole is located, remove any foreign objects with the pliers if present. Any object that is stuck in the tire, or puncturing the tire is considered a foreign object. These may include sharp objects, commonly seen as nails, screws, or pieces of metal.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_118541">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/sDERNAvdXWleGuqL.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/BprRAULMeQBQW42R.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/KFpcg2PRG6uuDvNJ.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_118542">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_118542"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Insert the reamer into the puncture. Work the reamer in and out while twisting to clean and prep the puncture.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_118542">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/MKrBMqytRTYaWCKY.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_118543">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_118543"/>
  <requiresTool rdf:resource="#tire_plug_kit"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Put the plug into the insertion tool.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_118543">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/MukIqYeEPEfYCtUk.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_118544">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_118544"/>
  <requiresTool rdf:resource="#tire_plug_kit"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">8</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Insert the plug into the punctured area, leaving about ¼ inch of the plug above the surface of the tire. Yank the insertion tool so the plug remains in the tire, but the tool releases. Cut the remaining plug so it is flush with the tire.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_118544">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/EWPM2LFdmBMYqNgt.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_118545">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_118545"/>
  <requiresTool rdf:resource="#tire_plug_kit"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Refill the tire to the proper pressure.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_118545">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/dDyJBpy33Kis3Gjv.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_118546">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_118546"/>
  <requiresTool rdf:resource="#air_pressure_gague"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">10</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use the air pressure gauge to make sure that the tire is back to its original air pressure.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_118546">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/W6prm11TnPypJtEP.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/CsXvcDAw2uN2b5Oq.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Procedure_22884">
  <rdf:type rdf:resource="#Procedure"/>
  <has_make rdf:resource="#Mercedes-Benz"/>
  <has_Item rdf:resource="#Mercedes_W123"/>
  <ProcedureForThis rdf:resource="#Mercedes_W123"/>
  <has_step rdf:resource="#Step_60029"/>
  <has_step rdf:resource="#Step_60030"/>
  <has_step rdf:resource="#Step_60031"/>
  <has_step rdf:resource="#Step_60032"/>
  <has_step rdf:resource="#Step_60033"/>
  <has_step rdf:resource="#Step_60034"/>
  <has_step rdf:resource="#Step_60035"/>
  <has_step rdf:resource="#Step_60036"/>
  <has_guideID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">22884</has_guideID>
  <guideTitle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mercedes W123 Sway Bar Link Replacement</guideTitle>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#17_mm_open_end_wrench">
  <rdf:type rdf:resource="#Tool"/>
  <toolURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">/Item/17_mm_Open_End_Wrench</toolURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_60029">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_60029"/>
  <requiresTool rdf:resource="#17_mm_open_end_wrench"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">You will need to begin by jacking up the rear end of your car, and removing the rear wheels. If you need help with these, see the guides below: [guide|22770|Jacking technique guide.] [guide|22883|Wheel removal technique guide]</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_60029">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/vFMIdM5KPdRWDZIM.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/ZKLaTOhP1HKI5eGW.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_60030">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_60030"/>
  <requiresTool rdf:resource="#17_mm_open_end_wrench"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">You can see here the sway bar link where it attaches to the sway bar. It's hidden partially behind the brake shield. It's tricky to take a good picture of... The threaded portion of the link goes through a hole in the sway bar end and is held in place by a nut.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_60030">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/KhLTIGN5A5EKCIWJ.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_60031">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_60031"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">At the bottom, it screws in to the hub assembly.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_60031">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/ELRWmpBoaV1aYUqk.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_60032">
  <rdf:type rdf:resource="#Step"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">View this video to hear what a bad sway bar link sounds like when you move the sway bar around. If yours sounds like this is definitely time to replace it.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_60033">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_60033"/>
  <requiresTool rdf:resource="#17_mm_open_end_wrench"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the nut from the top of the sway bar link using a 17 mm socket to turn off the nut while holding the inside of the sway bar link joint with an open end 17mm wrench. Then use the open end 17mm wrench to loosen the bottom joint of the sway bar from the hub assembly. You'll then be able to remove the sway bar link for inspection. Save the nut and any washers as the new links do not come with replacements.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_60033">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/ELoOVqZMvKRBWIAX.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_60034">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_60034"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pictured are example of the joints on the two sway bar links that were removed from this 1985 300D. Each one is split or even missing. You can see how this exposes the joint to moisture, dust, and road debris; this quickly ruins the joint.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_60034">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/HEEsFUP46TyLRUYi.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/FWLqHcOF5s26pmMZ.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/rI1LQYkTIcEyh3TI.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_60035">
  <rdf:type rdf:resource="#Step"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Watch this video to see how loose the bottom joint on this sway bar link was. It was half of the culprit for the clunking. The other side was just as bad.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_60036">
  <rdf:type rdf:resource="#Step"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Install the new sway bar links by following these steps generally in reverse. View this video to hear the difference with new links. Or, perhaps more accurately, not hear anything at all!</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Procedure_7301">
  <rdf:type rdf:resource="#Procedure"/>
  <has_make rdf:resource="#Honda"/>
  <has_model rdf:resource="#Honda_Accord"/>
  <has_Item rdf:resource="#1998-2002_Honda_Accord"/>
  <ProcedureForThis rdf:resource="#1998-2002_Honda_Accord"/>
  <has_step rdf:resource="#Step_31071"/>
  <has_step rdf:resource="#Step_31072"/>
  <has_step rdf:resource="#Step_31073"/>
  <has_step rdf:resource="#Step_31075"/>
  <has_step rdf:resource="#Step_31077"/>
  <has_step rdf:resource="#Step_31078"/>
  <has_step rdf:resource="#Step_31082"/>
  <has_step rdf:resource="#Step_31083"/>
  <has_guideID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">7301</has_guideID>
  <guideTitle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1998-2002 Honda Accord Tail Lights Replacement</guideTitle>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_31071">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_31071"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Open the trunk using your key in the trunk key slot or by using the lever under the driver's console.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_31071">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/YUVpxgnUTl5BCmjt.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/Z6haxYAVoyUWAqQt.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_31072">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_31072"/>
  <requiresTool rdf:resource="#flathead_screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the small plastic screw along the vertical edge of the trunk entrance using a flathead screwdriver. Carefully peel back the trunk liner from around the edge of trunk opening by the light. Locate the two panel tail light connectors located between the liner and the panel tail light assembly.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_31072">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/fVrtMONQfRJwECeS.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/QWYZOXGCfU34GRGI.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/umADZaAUxkZHpXOD.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_31073">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_31073"/>
  <requiresTool rdf:resource="#socket"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Grab a light bulb socket and turn it counterclockwise until it can be removed from its hole. Repeat this process for the other light bulb socket.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_31073">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/JwUTamFkaGgxBZ6v.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/M4EqBS5ScuUFoNZF.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/HRJ5K36fuFlmeSis.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_31075">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_31075"/>
  <requiresTool rdf:resource="#socket"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate the light bulb sockets in the trunk lid, located on the back side of the trunk tail lights. Turn the light bulb sockets counterclockwise and remove them from their holes.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_31075">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/l3fm5lH5YtQGZ5iA.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/VXS2Z6oOJ1kFJ5PT.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/sqmFVXj6yBml6vg2.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_31077">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_31077"/>
  <requiresTool rdf:resource="#socket_wrench"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the connector at the bottom of the panel tail light assembly. Locate the three 8 mm nuts holding the panel tail light assembly to the car and use an 8 mm socket wrench or box end wrench to remove the them.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_31077">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/NNAjKGgxhs4opC3J.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/dR26SS3Cw5IxaV2Z.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_31078">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_31078"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the panel tail light assembly by pulling it straight away from the car. If you are having troubles removing the light, try wiggling it back and forth as you pull it out.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_31078">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/5AHYfDbLjgfdHMSe.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_31082">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_31082"/>
  <requiresTool rdf:resource="#8_mm_deep_socket"/>
  <requiresTool rdf:resource="#socket_wrench"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the three 8 mm hex nuts securing the trunk tail light using a socket wrench or box end wrench. The black nut at the edge of the trunk lid has an extended round end that prevents a standard 8 mm socket from seating fully. You can use a regular socket if you can keep the bit level while rotating, but a deep socket is recommended to prevent stripping.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_31082">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/5KLMIKR4WCOVfDlJ.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/TnWeOJslxVPAArJf.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/ldJYu1dAdOYmirGV.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_31083">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_31083"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lift the trunk tail light up from the outer edge, being careful not to break the plastic locator on the inside edge.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_31083">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/ynFRQVJqGTpYxAsJ.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Procedure_21534">
  <rdf:type rdf:resource="#Procedure"/>
  <has_make rdf:resource="#Mercedes-Benz"/>
  <has_Item rdf:resource="#Mercedes_W123"/>
  <ProcedureForThis rdf:resource="#Mercedes_W123"/>
  <has_step rdf:resource="#Step_57526"/>
  <has_step rdf:resource="#Step_57527"/>
  <has_step rdf:resource="#Step_57528"/>
  <has_step rdf:resource="#Step_57529"/>
  <has_step rdf:resource="#Step_57530"/>
  <has_step rdf:resource="#Step_57531"/>
  <has_step rdf:resource="#Step_57532"/>
  <has_step rdf:resource="#Step_57533"/>
  <has_guideID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">21534</has_guideID>
  <guideTitle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mercedes W123 Brake Caliper, Rear Replacement</guideTitle>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_57526">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_57526"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">You will need to remove your wheels before you start. Before beginning on removal of the calipers you should drain the brake fluid from them. This is done through the bleed screw on each caliper. Loosen this and let it drain in to a proper drain pan.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_57526">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/B1KYjJhBM5O4oBri.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/h2nOt2YTKwnbXpxO.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_57527">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_57527"/>
  <requiresTool rdf:resource="#wrench"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Next you will need to remove the soft rubber brake hoses. You will have to start by disconnecting the rubber hose from the hard line connection up inside the fender well. Then you can loosen it from the caliper and remove the line. This takes a 14mm wrench for the soft hose and an 11 mm wrench for the brake line. For more information and tips on removing these lines, [guide|21345|see the brake hose replacement guide.] Speaking of the rubber hoses, this is a great time to replace them with new parts if they show signs of age since they are already off of the car.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_57527">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/Ra6gQo3MMBgpkxkw.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/ZwNIKOJp4Sblb5bd.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_57528">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_57528"/>
  <requiresTool rdf:resource="#socket_wrench"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Next use a large ratchet wrench with a 19mm socket to remove the two bolts that hold the caliper to the vehicle. These bolts are in very tight, and had thread locker on them from the factory, so you may need a very long wrench or a breaker bar in order to get enough leverage to remove these two bolts. Hold on to the caliper as you remove the second of the two bolts so it doesn't fall when you remove the bolt.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_57528">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/R6pHRVsERCxoJoGg.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_57529">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_57529"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">After you have completed your work on the caliper, such as removing the old pads, and cleaning them up, they can be re-installed. Or, if you have new calipers, this is the time to install them. Before installing the caliper bolts be sure to apply some thread locker to them. Use the blue thread locker than can be removed with hand tools, not the red thread locker as that needs heat to remove. Hold the caliper up on the rotor and line the holes in the caliper up with the bolt holes in the steering knuckle. Get the first bolt started by hand. Then, start the second bolt by hand as well. Doing so helps ensure they do not become cross-threaded.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_57529">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/VZqtBVgARIg6AUIX.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/2QqephUq5o5yvfJi.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_57530">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_57530"/>
  <requiresTool rdf:resource="#wrench"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Then tighten the bolts down until they seat against the caliper. Do not tighten them all the way down with the normal ratchet wrench.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_57530">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/fwkZpFBVKYOAESrW.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_57531">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_57531"/>
  <requiresTool rdf:resource="#torque_wrench"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Switch your 19mm socket to a large 1/2 inch drive torque wrench. Torque both of the caliper bolts down to 115 nm/85 lb-ft.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_57531">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/Grb2AbAW2jUxKj42.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_57532">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_57532"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Re-attach the brake hoses, starting at the calipers and then attaching them to the hard lines. Before you drive the car you must bleed your brakes of air since you opened up the system. Never drive the car without bleeding after you've worked on the lines, or calipers. [guide|21442|See the brake flush/bleeding guide.]</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_57532">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/R5g2UamrGR6CqIiP.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_57533">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_57533"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">An important note - if you are removing and replacing the front and rear calipers at the same time be sure to keep the front and rear bolts separate. This is because they are different designs. While you can use the rear bolts on the front calipers, the front bolts will not fit on the rear calipers. Front bolt, with a shoulder Rear bolt, no shoulder, all thread</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_57533">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/dafYtuZYjjRLnosj.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Procedure_6521">
  <rdf:type rdf:resource="#Procedure"/>
