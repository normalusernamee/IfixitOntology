</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_42985_B222nWX16XbEb4Yo.standard">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/B222nWX16XbEb4Yo.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_42986">
  <rdf:type rdf:resource="#Step"/>
  <has_image rdf:resource="#StepImage_42986_xtrrsXPMh3nCBSpw.standard"/>
  <has_image rdf:resource="#StepImage_42986_JGtPxVHjuXcRKF4A.standard"/>
  <requiresTool rdf:resource="#ph00_phillips_screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove one gold colored Phillips screw located in the center.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_42986_xtrrsXPMh3nCBSpw.standard">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/xtrrsXPMh3nCBSpw.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_42986_JGtPxVHjuXcRKF4A.standard">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/JGtPxVHjuXcRKF4A.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_42988">
  <rdf:type rdf:resource="#Step"/>
  <has_image rdf:resource="#StepImage_42988_4fHkIUniFSiSUoNH.standard"/>
  <has_image rdf:resource="#StepImage_42988_gBJaGDMwWFvDBDEd.standard"/>
  <has_image rdf:resource="#StepImage_42988_1TvLifAPDRmTiqxH.standard"/>
  <requiresTool rdf:resource="#tweezers"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">It is advisable to grab an old CD and push it half way into the CD-ROM drive until it is visible through the hole marked in the first picture. This will prevent the part that has to be removed in this step to fall down into the unit, with the risk of damaging the laser. The metal plate slides forward as shown in the first picture. Use a pair of tweezers to very carefully slide forward and then lift up and remove the metal plate together with the white plastic disk, if they fall into the unit you are going to have to take it apart completely ([guide|12405|using this guide]).</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_42988_4fHkIUniFSiSUoNH.standard">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/4fHkIUniFSiSUoNH.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_42988_gBJaGDMwWFvDBDEd.standard">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/gBJaGDMwWFvDBDEd.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_42988_1TvLifAPDRmTiqxH.standard">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/1TvLifAPDRmTiqxH.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_42987">
  <rdf:type rdf:resource="#Step"/>
  <has_image rdf:resource="#StepImage_42987_UXaHvPkSmBnedZQC.standard"/>
  <requiresTool rdf:resource="#high_content_rubbing_alcohol"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate the lens inside the CD-ROM drive. Make sure that there is no dust on or near the player and area you are working on. Dust can get on the lens and scratch it while you are cleaning. It is very important that the lens is not damaged because the lens is used to read the discs. Dip one end of a cotton swab in rubbing alcohol. Lightly dab the swab on the lens. Be very gentle. Let the lens dry before putting the unit back together.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_42987_UXaHvPkSmBnedZQC.standard">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/UXaHvPkSmBnedZQC.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Procedure_22837">
  <rdf:type rdf:resource="#Procedure"/>
  <has_make rdf:resource="#Mercedes-Benz"/>
  <has_Item rdf:resource="#Mercedes_W123"/>
  <ProcedureForThis rdf:resource="#Mercedes_W123"/>
  <has_step rdf:resource="#Step_59828"/>
  <has_step rdf:resource="#Step_59829"/>
  <has_step rdf:resource="#Step_59830"/>
  <has_step rdf:resource="#Step_59831"/>
  <has_step rdf:resource="#Step_59832"/>
  <has_step rdf:resource="#Step_59833"/>
  <has_step rdf:resource="#Step_59834"/>
  <has_guideID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">22837</has_guideID>
  <guideTitle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Disassembling Mercedes W123 Glove Box Liner</guideTitle>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_59828">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_59828"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Start by opening the glove box.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_59828">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/UT4JxYOu1orrLPXM.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_59829">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_59829"/>
  <requiresTool rdf:resource="#phillips_screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the two phillips screws in the glove box door catch, then remove it.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_59829">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/TtoaN4GPvKMiOLBw.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/LCwmxwSumKSHSD5D.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_59830">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_59830"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Gently pry apart the expansion plug in the glove box clips. There will be a row of them on top, and on the bottom. Once you've pried it out part of the way you can pull it the rest of the way out by hand, or gently with needle nose pliers.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_59830">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/6yaImIYUXNgAGEIs.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/nS3tNB21JqWLTbVG.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_59831">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_59831"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">You can then pry the clips out of the liner. Remove them all.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_59831">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/LdJ33xFFbUXahjZC.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_59832">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_59832"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Gently pry down the cover for the glove box lamp.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_59832">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/OsMVYGLvwrItXCFM.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_59833">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_59833"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Now unplug the two connections from the lamp and remove the lamp housing. Be sure to make note of the orientation of the two wires for re-installation.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_59833">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/ZFDKDhRKu2Rf4mrB.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_59834">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_59834"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Now the glove box liner will pull out by hand.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_59834">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/4btBYStpW2PBMl31.standard</toolImageURL>
</owl:NamedIndividual>

