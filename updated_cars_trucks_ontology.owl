  <requiresTool rdf:resource="#socket_wrench"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a socket wrench or impact gun to remove the 15 mm nut on the front sway bar endlink.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_31568">
  <rdf:type rdf:resource="#StepImage"/>
