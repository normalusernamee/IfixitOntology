  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a plastic prying tool or spudger to gently remove nut cap from wiper base.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_73790">
  <rdf:type rdf:resource="#StepImage"/>
