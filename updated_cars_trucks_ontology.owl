  <requiresTool rdf:resource="#flathead_screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the cover on the door grip by prying gently with a flathead screwdriver.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_128967">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/QoWrAbcArgTTFYEd.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/hXfmrVvrugJOOj6y.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_128968">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_128968"/>
  <requiresTool rdf:resource="#t20_torx_screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Take off the inside door panel by removing all screws with a T20 Torx screwdriver. There are nine screws in total: two on the front, three on the outer side, two on bottom, and two on the inner side.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_128968">
  <rdf:type rdf:resource="#StepImage"/>
