</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_52641">
  <rdf:type rdf:resource="#Step"/>
  <has_image rdf:resource="#StepImage_52641_Rjwn2uio1N6VZOPi.standard"/>
  <has_image rdf:resource="#StepImage_52641_G1strlOb1HKeKEUN.standard"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The bottom cover is separated from the main PCA by removing the 5 screws seen in the top view.</stepText>
</owl:NamedIndividual>

