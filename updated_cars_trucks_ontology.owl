<owl:NamedIndividual rdf:about="#Step_59620">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_59620"/>
  <requiresTool rdf:resource="#motive_power_bleeder"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Begin by filling your power bleeder with a bottle, or two, of brake fluid.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_59620">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/DESwc5o3DiHDd1BP.standard</toolImageURL>
