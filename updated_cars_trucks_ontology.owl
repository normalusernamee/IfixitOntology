<owl:NamedIndividual rdf:about="#12_mm_socket">
  <rdf:type rdf:resource="#Tool"/>
  <toolURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">/Item/12_mm_Socket</toolURL>
  <toolURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.amazon.com/dp/B0002NYD7G/</toolURL>
