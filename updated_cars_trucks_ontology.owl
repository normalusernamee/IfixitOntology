  <has_step rdf:resource="#Step_17393"/>
  <has_step rdf:resource="#Step_17373"/>
  <has_step rdf:resource="#Step_17374"/>
  <has_step rdf:resource="#Step_17375"/>
  <has_step rdf:resource="#Step_17376"/>
  <has_step rdf:resource="#Step_17377"/>
  <has_guideID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">3636</has_guideID>
  <guideTitle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1998-2002 Honda Accord Distributor Rotor Replacement</guideTitle>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#.25_socket_wrench">
  <rdf:type rdf:resource="#Tool"/>
  <toolURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.amazon.com/Stanley-91-928-4-Inch-Release-Ratchet/dp/B000NJ7U1A/ref=sr_1_6?ie=UTF8&amp;qid=1390863167&amp;sr=8-6&amp;keywords=1%2F4%22+ratchet</toolURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#8_mm_deep_socket">
  <rdf:type rdf:resource="#Tool"/>
  <toolURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.amazon.com/TEKTON-1220-Socket-5-5-12mm-Sockets/dp/B000NPUJF8/ref=sr_1_1?ie=UTF8&amp;qid=1390863785&amp;sr=8-1&amp;keywords=8+mm+deep+socket</toolURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#probe_and_pickset">
  <rdf:type rdf:resource="#Tool"/>
  <has_image rdf:resource="#ToolImage_probe_and_pickset"/>
  <toolURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.ifixit.com/Tools/Probe-and-Pick-Set/IF145-059</toolURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#ToolImage_probe_and_pickset">
  <rdf:type rdf:resource="#ToolImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://da2lh5cs8ikqj.cloudfront.net/cart-products/1fZMHbcDB1yWGpkp.mini</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_17372">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_17372"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Before disconnecting the spark plug wires, be sure to trace their orientation from the individual cylinders back to the distributor cap. The cylinders are ordered from left to right when viewing the engine from the front of the car, with cylinder #1 being closest to the passenger side of the vehicle. Mark the order of the spark plug wires on the new distributor cap with a silver sharpie marker -- it will make reinstallation much easier.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_17372">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/QKH6jPOupnCZUAeF.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/TwNbwO5Ql3ZBrUxm.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/32tjeienbVPOqCML.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_17393">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_17393"/>
  <requiresTool rdf:resource="#socket"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Disconnect the four spark plug wires from the distributor cap by pulling their connectors straight away from the sockets on the distributor cap. It may be helpful to slightly twist the wires as you pull them.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_17393">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/EHAnIIsJMwKlOLhO.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#socket">
  <rdf:type rdf:resource="#Tool"/>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_17373">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_17373"/>
  <requiresTool rdf:resource="#8_mm_deep_socket"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a 1/4" drive, 8 mm deep socket to remove the three hex bolts securing the distributor cap to the engine. The position of the third bolt is not visible in the second picture.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_17373">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/ltXlLXGBApWs2IAb.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/do3eRlTYWFOrlBvU.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_17374">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_17374"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull the distributor cap away from the engine and set it aside.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_17374">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/GnM4JhWXOfr1WOXH.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_17375">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_17375"/>
  <requiresTool rdf:resource="#phillips_2_screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a dental pick or a small screwdriver to gently remove the D-shaped o-ring around the distributor cap flange. Install a new o-ring in the seal recess, making sure that it is sitting flush against the flange.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_17375">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/QsJ2GpRluWRFo4vH.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/TwTTFeLGpWdqXDuL.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_17376">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_17376"/>
  <requiresTool rdf:resource="#phillips_2_screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the single #2 Phillips screw securing the distributor rotor to the shaft.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_17376">
  <rdf:type rdf:resource="#StepImage"/>
