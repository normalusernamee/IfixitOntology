  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/JAbZfRCjuKEHTHv4.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/HuXA6aZIpqmWOhcH.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Procedure_8197">
  <rdf:type rdf:resource="#Procedure"/>
  <has_make rdf:resource="#Volvo"/>
  <has_Item rdf:resource="#1986-1993_Volvo_240"/>
  <ProcedureForThis rdf:resource="#1986-1993_Volvo_240"/>
  <has_step rdf:resource="#Step_33105"/>
  <has_step rdf:resource="#Step_33108"/>
  <has_step rdf:resource="#Step_33126"/>
  <has_guideID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">8197</has_guideID>
  <guideTitle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1986-1993 Volvo 240 Wheel Adapters Replacement</guideTitle>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_33108">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_33108"/>
  <requiresTool rdf:resource="#impact_wrench"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Place the wheel adapter over the existing wheel lugs, then install and hand-tighten the provided lug nuts. Tighten the lug nuts to 85 ft-lbs with a torque wrench or impact gun. The choice is yours as to what adapters you'd like to install. We chose 5x4.75 adapters so that we could equip our Volvo 240 with C4 Corvette wheels!</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_33108">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/fsJxDApbXILawEKK.standard</toolImageURL>
