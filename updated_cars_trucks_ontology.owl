  <has_image_for_step rdf:resource="#StepImage_86850"/>
  <requiresTool rdf:resource="#flathead_screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a flathead screwdriver (or similar tool) to lift the socket clip of the tab on the bulb and remove the bulb. Change the bulb and the result will be fully functional headlights.</stepText>
</owl:NamedIndividual>

