</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_92548">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_92548"/>
  <requiresTool rdf:resource="#ph0_phillips_screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lay the key down so that the Honda symbol is facing upward. Use a Phillips #0 screwdriver to remove one screw.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_92548">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/BGlMLgcABiwVDUDt.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/UWWBoXakDgYE2tIR.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_92549">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_92549"/>
  <requiresTool rdf:resource="#spudger"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Insert the flat end of the Spudger into the side of the key remote. Pry the key remote apart to remove the cover.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_92549">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/BHGNmsEu3hwFnvIg.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/YhYJhTmcwMKYi3wt.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_92550">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_92550"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the existing battery. Insert a new CR1616 battery with the '+' sign facing upward.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_92550">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/XJJhFjpIwRjadMCD.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_92551">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_92551"/>
  <requiresTool rdf:resource="#ph0_phillips_screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Place the two halves of the remote cover back together. Press the unlock or lock buttons to make sure the remote is working. Place the screw into the hole and tighten with a Phillips  #0 screwdriver.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_92551">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/LISbUgPnPHJLWJKO.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Procedure_20578">
  <rdf:type rdf:resource="#Procedure"/>
  <has_make rdf:resource="#Mercedes-Benz"/>
  <has_Item rdf:resource="#Mercedes_W123"/>
  <ProcedureForThis rdf:resource="#Mercedes_W123"/>
  <has_step rdf:resource="#Step_56564"/>
  <has_step rdf:resource="#Step_56565"/>
  <has_step rdf:resource="#Step_56566"/>
  <has_step rdf:resource="#Step_56567"/>
  <has_step rdf:resource="#Step_56568"/>
  <has_guideID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">20578</has_guideID>
  <guideTitle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mercedes W123 Fuel Filler Neck Seal Replacement</guideTitle>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_56564">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_56564"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">When inspecting your fuel filler neck seal look for dryness, brittleness, or like on this particular diesel W123 obvious damage. This was caused by drips of B99 biodiesel on the already aged rubber seal.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_56564">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/5Rlu5JOJIFISP3ap.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_56565">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_56565"/>
  <requiresTool rdf:resource="#flathead_screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Gently pry around the edge of the old seal with a flat blade screwdriver or a large plastic spudger type tool. The old seals often stick slightly to the car.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_56565">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/EBPhRf45mPHctWYO.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_56566">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_56566"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pull the seal off of the fuel filler neck. It will look like this once the seal is off.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_56566">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/ZCBopUBTQSmiWyoB.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_56567">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_56567"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use your thumb to press the new seal in around the filler neck. The new seal has a lip on the back that must be pressed in to the hole around the neck. This lip will grip the metal edge of the hole for a tight seal.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_56567">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/BTCheVrEc2tJaJwJ.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/5cRPsUWtts611Whv.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_56568">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_56568"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Here's one last look at the damage done by the biodiesel. The owner of this W123 will be more careful in the future not to drip fuel on the filler neck seal when filling with B99; at that concentration, biodiesel is very caustic to rubber. Lesson learned!</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_56568">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/SMLFkHt6FGMAWlAS.standard</toolImageURL>
</owl:NamedIndividual>

