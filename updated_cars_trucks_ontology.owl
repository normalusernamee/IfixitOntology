  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove tires by pulling directly away from car exposing rear brake disks and calipers.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_18254">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/ZXoUA6eWKB6pQRnH.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_18255">
  <rdf:type rdf:resource="#Step"/>
