  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/wcTLcEiqVu5ax6OF.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Procedure_22838">
  <rdf:type rdf:resource="#Procedure"/>
  <has_make rdf:resource="#Mercedes-Benz"/>
  <has_Item rdf:resource="#Mercedes_W123"/>
  <ProcedureForThis rdf:resource="#Mercedes_W123"/>
  <has_step rdf:resource="#Step_59837"/>
  <has_step rdf:resource="#Step_59838"/>
  <has_step rdf:resource="#Step_59839"/>
  <has_step rdf:resource="#Step_59841"/>
  <has_step rdf:resource="#Step_59843"/>
  <has_guideID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">22838</has_guideID>
  <guideTitle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Disassembling Mercedes W123 Kick Panel</guideTitle>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_59837">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_59837"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Begin by gently prying out the covers for the screws on the upper edge of the kick panel.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_59837">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/kwuRobYQQRBQJxRo.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_59838">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_59838"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Then remove these screws.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_59838">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/STihXWhAJOQiGZZW.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_59839">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_59839"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Then remove any metal screws, and any plastic trim screws, at the sides of the kick panel below the dash.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_59839">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/TkUR1JVvTRkQXIHm.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/Q1OYTViAkycGs3yX.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_59841">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_59841"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Now lower the panel down and rest if on the floor. It can be removed full for further ease of access, depending on the job at hand.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_59841">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/TDQSfTjdttZuO2sa.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/dMeJutA5CvtW2DBF.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_59843">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_59843"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Now you'll want to identify some of the components you are looking at so you can begin working on the one that is having issues. Here are some of the ones you may find. This car is a 1985 300D; if you have an older model the items are likely a bit different. Heater blower motor Over-voltage protection relay Air-conditioning expansion valve</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_59843">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/NL5ihcUJuOWPMvRR.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/mZfpDntaVjvLkCZd.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/TEO4VRyrB3jvVjCY.standard</toolImageURL>
</owl:NamedIndividual>

