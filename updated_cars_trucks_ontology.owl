</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Honda_Civic">
  <rdf:type rdf:resource="#Model"/>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#2012-2015_Honda_Civic">
