<owl:NamedIndividual rdf:about="#1_4_socket">
  <rdf:type rdf:resource="#Tool"/>
  <toolURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">/Item/1_4%22_socket_wrench</toolURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109326">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109326"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate the hood latch in your vehicle located on the drivers side under the dash to the left of the parking brake pedal. Pull the latch to release hood.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109326">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/E6EICaB4R6KTsSGS.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/f5wwbadebkd4Eiku.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109331">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109331"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate the hood release latch in between the grille and hood. You will find this release latch near the center of the hood in the small gap between the hood and grille. Lift up on the latch then raise the hood.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109331">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/lnPMaMSOOowILyay.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/JBAdSaAFWMKs6lXF.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/KfbyaiRbQWG3cLbW.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109332">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109332"/>
  <requiresTool rdf:resource="#socket_wrench"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate your battery. It's in the  front of the engine compartment, on the drivers side. Remove the negative terminal with 5/16" socket or by using a 5/16" wrench. BE CAREFUL. DO NOT CROSS POSITIVE AND NEGATIVE WIRES.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109332">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/rYsMtIrJiCyNrIfN.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/RGgToRCZXCpXvUNC.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109337">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109337"/>
  <requiresTool rdf:resource="#1_4_socket"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Make sure the vehicle is on a flat surface, then use the parking brake. Shift vehicle into 1st gear so the shifter will be out of the way for the next step.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109337">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/AxB4minnHi2KCh45.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/gZo264xwFqEyNKKo.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109340">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109340"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Firmly grasp the dash trim and pull it towards you. Once it is removed from its clips, pull up and the trim will come out the rest of the way. Set the trim aside; you are ready for the next step.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109340">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/ioJwedPUXhsPc6GS.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/HaTkT5SbaGa6OtDV.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/xryHN3HPg6xVhOuv.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109345">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109345"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the face plate from the head unit. To do this, pull out on both the left and right sides to release the head unit from its clips. After being released from the clips, set the face plate aside and continue to the next step.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109345">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/jedUYdirOam2BsTK.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/sV3rCFdGYCnMMKyM.standard</toolImageURL>
