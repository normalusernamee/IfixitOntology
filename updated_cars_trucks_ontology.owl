</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_165168">
  <rdf:type rdf:resource="#Step"/>
  <has_image rdf:resource="#StepImage_165168_3Xlq2eOEJaNrVURX.standard"/>
  <has_image rdf:resource="#StepImage_165168_gjdJAIvYek2HOSbb.standard"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a spudger or prying tool to open the back of the face plate. Pry the back at multiple points around the edge of the face plate. This will take some patience. Applying too much force risks breaking the plastic.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_165168_3Xlq2eOEJaNrVURX.standard">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/3Xlq2eOEJaNrVURX.standard</toolImageURL>
</owl:NamedIndividual>

