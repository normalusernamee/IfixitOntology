  <requiresTool rdf:resource="#12_v_bulb_and_socket"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">If the bulb lights up, the power supply to the clock is  good.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_65846">
