<owl:NamedIndividual rdf:about="#Step_58883">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_58883"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">To start the process of changing your power steering fluid, you'll need to remove the lid on the pump reservoir. If you want guidance on this, and on checking the fluid level, [guide|20570|see the guide on checking power steering fluid level.]</stepText>
</owl:NamedIndividual>

