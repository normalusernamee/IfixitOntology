  <guideTitle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">2001 Volkswagen Passat Key Fob Battery Replacement</guideTitle>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#cr2032_lithium_battery">
  <rdf:type rdf:resource="#Tool"/>
  <has_image rdf:resource="#ToolImage_cr2032_lithium_battery"/>
  <toolURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.ifixit.com/Apple-Parts/CR2032-Lithium-Battery/IF207-011</toolURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#ToolImage_cr2032_lithium_battery">
  <rdf:type rdf:resource="#ToolImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://da2lh5cs8ikqj.cloudfront.net/cart-products/UAjjP1aaaQBXcVLb.mini</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_130638">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_130638"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Press the release button on the key fob so that key is extended. Locate the small indentation in the middle of the key fob.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_130638">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/TN1iCEc3wwADBxav.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/rDFZPGmn1VBGXsWs.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_130646">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_130646"/>
  <requiresTool rdf:resource="#2.5mm_flathead_screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Place a screwdriver in the indentation of the key fob. Apply pressure and rotate the screwdriver until key fob begins to separate. Using light force, pull the key fob apart. Remove the cover on lower part of the key fob and locate the battery.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_130646">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/J5DhKOWaABYpox2C.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/hQiRin6GFWdyxQbi.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/FkQSHU1ZyOQNYSsk.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_130648">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_130648"/>
  <requiresTool rdf:resource="#2.5mm_flathead_screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a screwdriver to remove the battery. Place a new battery in the key fob and press down gently to secure it in place.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_130648">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/O1NYuXhRCQlKaERw.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/tqN2VkVORjpMMV5E.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/jqI3kZaO6WrLrKPo.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_130651">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_130651"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pick up the pieces of the key fob and begin to slide the pieces together. This needs to be done slowly to not break the plastic on the fob.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_130651">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/bxIxyEvkUVnftCd1.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/PmQuW1gtVmTPNZZK.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_130653">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_130653"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Press the key release button to make sure that the key fob operates correctly. Press the lock, unlock and trunk buttons and check for the red indicator light to make sure the key fob working properly. Depending on your vehicle, you may need to reprogram your key fob to your car. Either consult a dealership or review your owner's manual on how to reprogram your key fob.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_130653">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/aUssaSNFhZth4NqD.standard</toolImageURL>
</owl:NamedIndividual>

