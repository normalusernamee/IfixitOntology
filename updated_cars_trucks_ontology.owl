  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/hdYIRnekSuBrJZK5.standard</toolImageURL>
</owl:NamedIndividual>

