</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_93151">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_93151"/>
  <requiresTool rdf:resource="#jack_hook"/>
  <requiresTool rdf:resource="#lug_wrench"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Insert jack hook into loop on jack. Insert end of jack hook into lug wrench. To raise vehicle turn the lug wrench, attached to jack hook, clockwise.  Turn until tire is one inch (approximately 30mm) off ground.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_93151">
