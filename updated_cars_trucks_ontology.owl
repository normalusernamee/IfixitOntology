  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use hydraulic jack on frame of car to raise car. Place jack head underneath frame couple (pictured). Only necessary to jack the car until the wheels are just off the ground.</stepText>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</stepOrder>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_18252">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/MOLCJmdp3REPGt2q.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/DjFGFEpuZSrN3HgO.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_18253">
  <rdf:type rdf:resource="#Step"/>
