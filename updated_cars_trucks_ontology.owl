  <has_step rdf:resource="#Step_109855"/>
  <has_step rdf:resource="#Step_109869"/>
  <has_step rdf:resource="#Step_109875"/>
  <has_step rdf:resource="#Step_109879"/>
  <has_step rdf:resource="#Step_109880"/>
  <has_guideID rdf:datatype="http://www.w3.org/2001/XMLSchema#string">50458</has_guideID>
  <guideTitle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">2007-2012 Jeep Wrangler Headlight Replacement</guideTitle>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#star_bit_screwdriver">
  <rdf:type rdf:resource="#Tool"/>
  <toolURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">http://www.harborfreight.com/6-piece-star-bit-screwdriver-set-94757.html</toolURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109220">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109220"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate the two gray latches on either side of the hood near the headlights. Pull the latches away from the hood to release the latches.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109220">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/YysT6eTllmtaBZoB.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/ZTKREAlfEiBWVOmA.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109767">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109767"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Reach under the hood and push the lever left to right.  You should now be able to lift the hood. Prop open the hood by inserting the support rod into the designated hole on the underside of the hood.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109767">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/fdTHMpME3lmHvbY6.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/qwJNWXx62NYxOD2G.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109853">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109853"/>
  <requiresTool rdf:resource="#flathead_screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate all six push on fasteners on the top of the grill. Use a flathead screwdriver to pry up the fasteners, and make sure you put the screws somewhere safe. If the fasteners will not come up, use two flathead screwdrivers on either side of the fastener to remove.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109853">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/ymYFFHZBOknDXVY6.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/SupdHZucH2A4IdLH.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/3SXVHM5tNVTmUTpH.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109855">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109855"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate right and left blinker lights under the fenders. Twist blinker light toward the ground, and make sure you pull the bulb out carefully. Make sure you are handling bulb carefully.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109855">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/MfjiT1ZWogkcfDFj.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/WFPh4HywTpI2Wlb5.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/Ydapi3IvhliSbOMc.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109869">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109869"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Make sure all fasteners are removed from top of the grill, and the top of the grill is loose. Firmly pull the bottom of the grill until the grill disconnects from the jeep.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109869">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/ZjyrlCC1MYOp2Pr2.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/Tdb3LFT2YBETJQtA.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/MmwjlpJqnkbYRvoB.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109875">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109875"/>
  <requiresTool rdf:resource="#star_bit_screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate the 4 screws holding the headlight in place. Use a star bit screwdriver to unscrew the .164-18 x .625 mm star bit screws around the headlight, and make sure you place screws somewhere safe. Hold the headlight when unscrewing last screw to prevent the headlight from falling.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109875">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/pVYwRi4lDAFguasE.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/rDSJGLSMexGUGy6j.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109879">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109879"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use your hands to carefully remove silver ring from the headlight.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109879">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/iMn4R3sSuINboAWb.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/oAcAwjtUWDVu2SMa.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109880">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109880"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate red tab on the headlight plug connector, and push it away from the headlight. Firmly pull the connector away from headlight to remove.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109880">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/xrjPIQNZVHvdCwlW.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/11AZVPqtFSaaEAhl.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/4lSFkDdF22USvh1X.standard</toolImageURL>
</owl:NamedIndividual>

