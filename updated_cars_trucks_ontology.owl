  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/oOOhy2WBOqDvCwZw.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_131924">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_131924"/>
  <requiresTool rdf:resource="#phillips_2_screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">You should now be able to see the recifier assembly, the brushes are located in the middle of the recifier assembly. Remove the brush assembly by unscrewing 2, 6mm center screws using a #2 Phillips head screw driver</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_131924">
