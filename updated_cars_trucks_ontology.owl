  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Locate the two rear brake pad retaining pins on each caliper.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_18264">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/yBAKEGpbEPewYvAk.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_18266">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_18266"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">11</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove brake pad retaining pins by punching them out with a 3mm allen key.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_18266">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/GDiu3PGIenTX3JyN.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_18269">
