<owl:NamedIndividual rdf:about="#Step_166603">
  <rdf:type rdf:resource="#Step"/>
  <has_image rdf:resource="#StepImage_166603_Y2brD1tpH3MuMwaG.standard"/>
  <has_image rdf:resource="#StepImage_166603_4LagHLxjJCvRayY1.standard"/>
