</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_132630">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_132630"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">9</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a c-clamp or another fixture to hold the pulley in place Be gentle with the clamp. Excessive force will bend the pulley out of shape</stepText>
</owl:NamedIndividual>

