</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_54076">
  <rdf:type rdf:resource="#Step"/>
  <has_image rdf:resource="#StepImage_54076_YHxKBRALeFYtPUfE.standard"/>
  <has_image rdf:resource="#StepImage_54076_qkTpSLWEmbBVWQW4.standard"/>
  <has_image rdf:resource="#StepImage_54076_3NW5UIRaHXStEOU4.standard"/>
  <requiresTool rdf:resource="#needlenose_pliers"/>
  <requiresTool rdf:resource="#spudger"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">18</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a spudger to disconnect the connector from the board. Flip over the front panel and pull off the rubber knob of the rotary push button. Remove the 11 mm nut with a pair of pliers or a socket wrench if you have one that is long enough.</stepText>
</owl:NamedIndividual>

