</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_58884">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_58884"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">With the lid removed you can see that this fluid is a bit low, and is also very dark/dirty.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_58884">
