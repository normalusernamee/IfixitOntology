<owl:NamedIndividual rdf:about="#Step_97400">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_97400"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Pry up the plastic tab from inside the  handle located on the front door armrest.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_97400">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/wu3DVKaMOaJqN1js.standard</toolImageURL>
