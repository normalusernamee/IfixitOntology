<owl:NamedIndividual rdf:about="#Step_56865">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_56865"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">16</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Now you can remove the gear and cable housing. It seems to work best to rotate the housing up and in the direction of the antenna shaft opening before angling it up and pulling it out of the motor body.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_56865">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/aiFO2nNHSP6PDRGe.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_56866">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_56866"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">17</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">This leaves the lower cable retainer in place. Lift it up and out. Note the thin metal washer at the center of this.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_56866">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/6ujJiqJbpCivLRHx.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/55cwMV36EiwhQVhT.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_56867">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_56867"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">18</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the three rubber buffers. Remove the gear shaft.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_56867">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/y5BmfYIjyupOtUGH.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/1UkS6EByIy4sZuCq.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/P2Px6tAXMrfsbF4U.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_56868">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_56868"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">19</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">You now have an empty motor housing, besides the control board. This disassembly does not cover the electric motor as everything is in plain sight. Worm gear Motor Small belt driven pulleys for worm gear</stepText>
</owl:NamedIndividual>

