  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Turn car off and remove keys from ignition. Place car in park (automatic) or neutral (manual). Engage parking brake.</stepText>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</stepOrder>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_18247">
