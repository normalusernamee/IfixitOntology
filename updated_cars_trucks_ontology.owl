</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_191417">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_191417"/>
  <requiresTool rdf:resource="#spark_plug_socket"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">There are four 8mm bolts holding down the spark plug cover Gently lift it out of the way</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_191417">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/oNQt4AO16ONyrPLh.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/EJ3QkhuJCNnS1f6b.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_191418">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_191418"/>
  <requiresTool rdf:resource="#screwdriver"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">6</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The connector for the VCT Solenoid will need to be unplugged There is a small metal tab holding the connector on Remove the tab with a small flat head screw driver underneath it Be careful not to lose it! The connector will just pull off Pull the wire back out of the way</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_191418">
  <rdf:type rdf:resource="#StepImage"/>
