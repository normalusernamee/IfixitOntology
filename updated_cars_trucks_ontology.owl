  <rdf:type rdf:resource="#Tool"/>
  <toolURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://www.ifixit.com/Store/Parts/Digital-Multimeter/IF145-035</toolURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_52254">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_52254"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">0</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Start removing all the mark parts in red circles. (upper bolts from the intake manifold,hoses,vacuum hoses,filter tube,etc)</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_52254">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/wfBhZUywFvRfpMSJ.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_52255">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_52255"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Remove the connector from the PCM</stepText>
</owl:NamedIndividual>

