  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/OvHPmM2IsTWZg3hC.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_31570">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_31570"/>
  <requiresTool rdf:resource="#tie_rod_end_puller"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">3</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use a socket wrench or impact gun to remove the 19 mm tie rod end nut.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_31570">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/2ruIOgZ1BVyK2iXH.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/mmmVwvLT3Yy4bEZb.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_31572">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_31572"/>
  <requiresTool rdf:resource="#tie_rod_end_puller"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Place a tie rod end puller over the exposed threads of the tie rod end and turn the puller with a socket wrench to push the tie rod end out of the steering knuckle.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_31572">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/jxfGi6mfQnIWNQqQ.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/143ZqueNVRemk1jj.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_31573">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_31573"/>
  <requiresTool rdf:resource="#channellock_groove_joint_pliers"/>
  <requiresTool rdf:resource="#wrench"/>
  <requiresTool rdf:resource="#18_mm_box_end_wrench"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">The ball joint is held to the control arm by three 18 mm bolts. Remove the bottom two nuts with a breaker bar or long wrench. Place a breaker bar over the gold nut on the bottom of the control arm. The breaker bar alone will not be able to loosen the nut, since the bolt is free to rotate. Place an 18 mm box end wrench over the bolt on top of the control arm. Turn the breaker bar and the wrench in their respective counterclockwise directions to loosen and remove the third ball joint-to-control arm bolt.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_31573">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/XNJhQtLL3vRYbUpT.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/AFTfnEJ3niifugZ6.standard</toolImageURL>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/cifYGLtqnJmbqkYW.standard</toolImageURL>
</owl:NamedIndividual>

