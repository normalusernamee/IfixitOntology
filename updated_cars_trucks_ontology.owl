</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_109497">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_109497"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Replace with a new battery. Secure the battery cover back in place.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_109497">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/OqZF1bDpZrBbYrnE.standard</toolImageURL>
</owl:NamedIndividual>

