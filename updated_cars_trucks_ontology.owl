  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/gjdJAIvYek2HOSbb.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_165170">
  <rdf:type rdf:resource="#Step"/>
  <has_image rdf:resource="#StepImage_165170_hMxUlMQAEXX4cp5V.standard"/>
  <has_image rdf:resource="#StepImage_165170_GvLvtsB6IWIJUYCh.standard"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">4</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Lift and remove the motherboard to reveal the inside of the front panel. You might need to pry the right side up with the spudger.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_165170_hMxUlMQAEXX4cp5V.standard">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/hMxUlMQAEXX4cp5V.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_165170_GvLvtsB6IWIJUYCh.standard">
  <rdf:type rdf:resource="#StepImage"/>
  <toolImageURL rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://d3nevzfk7ii3be.cloudfront.net/igi/GvLvtsB6IWIJUYCh.standard</toolImageURL>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_165172">
  <rdf:type rdf:resource="#Step"/>
  <has_image rdf:resource="#StepImage_165172_ZiFjMVADA3aO1bLK.standard"/>
  <has_image rdf:resource="#StepImage_165172_KvqS4k6PZ5TXuk5W.standard"/>
  <has_image rdf:resource="#StepImage_165172_DRr6GYgUXURnbx6I.standard"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">5</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Use the plastic spudger to straighten out the six pegs to allow the removal of the LCD screen. Make sure to use an [https://en.wikipedia.org/wiki/Electrostatic_discharge|ESD]-safe prying tool when working on electrical boards. Here, we are using a plastic tool, not a metal one.</stepText>
</owl:NamedIndividual>

