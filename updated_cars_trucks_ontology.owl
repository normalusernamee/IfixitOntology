</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#Step_93159">
  <rdf:type rdf:resource="#Step"/>
  <has_image_for_step rdf:resource="#StepImage_93159"/>
  <requiresTool rdf:resource="#lug_wrench"/>
  <stepOrder rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">7</stepOrder>
  <stepText rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Once tire is elevated, remove lug nuts by hand. Be aware that the tire will shift as lug nuts are removed. After removing lug nuts, carefully take tire off of frame.</stepText>
</owl:NamedIndividual>

<owl:NamedIndividual rdf:about="#StepImage_93159">
