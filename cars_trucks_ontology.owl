</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#has_step">
  <rdfs:domain rdf:resource="#Procedure"/>
  <rdfs:range rdf:resource="#Step"/>
</owl:ObjectProperty>

<owl:DatatypeProperty rdf:about="#has_guideID">
  <rdfs:domain rdf:resource="#Procedure"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#guideTitle">
  <rdfs:domain rdf:resource="#Procedure"/>
  <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
</owl:DatatypeProperty>

<owl:DatatypeProperty rdf:about="#stepOrder">
