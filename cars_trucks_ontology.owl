</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#procedure_for">
  <rdfs:domain rdf:resource="#Procedure"/>
  <rdfs:range rdf:resource="#Item"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#has_make">
  <rdfs:domain rdf:resource="#Procedure"/>
  <rdfs:range rdf:resource="#Make"/>
  <owl:inverseOf rdf:resource="#is_for_make"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#has_model">
  <rdfs:domain rdf:resource="#Procedure"/>
  <rdfs:range rdf:resource="#Model"/>
  <owl:inverseOf rdf:resource="#is_for_model"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#is_for_make">
  <rdfs:domain rdf:resource="#Make"/>
  <rdfs:range rdf:resource="#Procedure"/>
  <owl:inverseOf rdf:resource="#has_make"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#is_for_model">
  <rdfs:domain rdf:resource="#Model"/>
  <rdfs:range rdf:resource="#Procedure"/>
  <owl:inverseOf rdf:resource="#has_model"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#has_model_for_make">
  <rdfs:domain rdf:resource="#Make"/>
  <rdfs:range rdf:resource="#Model"/>
  <owl:inverseOf rdf:resource="#is_model_of"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#is_model_of">
  <rdfs:domain rdf:resource="#Model"/>
  <rdfs:range rdf:resource="#Make"/>
  <owl:inverseOf rdf:resource="#has_model_for_make"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#hasProcedure">
  <rdfs:domain rdf:resource="#Item"/>
  <rdfs:range rdf:resource="#Procedure"/>
