</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#requiresTool">
  <rdfs:domain rdf:resource="#Step"/>
  <rdfs:range rdf:resource="#Tool"/>
  <owl:inverseOf rdf:resource="#isRequiredFor"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isRequiredFor">
  <rdfs:domain rdf:resource="#Tool"/>
  <rdfs:range rdf:resource="#Step"/>
  <owl:inverseOf rdf:resource="#requiresTool"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#has_part">
  <rdfs:domain rdf:resource="#Item"/>
  <rdfs:range rdf:resource="#Item"/>
  <owl:inverseOf rdf:resource="#part_of"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#part_of">
  <rdfs:domain rdf:resource="#Item"/>
