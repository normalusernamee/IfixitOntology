<?xml version="1.0"?>
<rdf:RDF xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
         xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
         xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
         xmlns:owl="http://www.w3.org/2002/07/owl#"
         xml:base="http://example.org/ifixit.owl"
         xmlns="http://example.org/ifixit.owl#">

<owl:Ontology rdf:about="http://example.org/ifixit.owl"/>

<owl:ObjectProperty rdf:about="#requires_tool">
  <rdfs:domain rdf:resource="#Procedure"/>
  <rdfs:range rdf:resource="#Tool"/>
  <owl:inverseOf rdf:resource="#used_in"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#used_in">
  <rdfs:domain rdf:resource="#Tool"/>
  <rdfs:range rdf:resource="#Procedure"/>
