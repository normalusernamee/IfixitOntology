</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#ProcedureForThis">
  <rdfs:domain rdf:resource="#Procedure"/>
  <rdfs:range rdf:resource="#Item"/>
  <owl:inverseOf rdf:resource="#hasProcedure"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#has_Item">
  <rdfs:domain rdf:resource="#Procedure"/>
  <rdfs:range rdf:resource="#Item"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#isRepairGuideFor">
  <rdfs:domain rdf:resource="#Procedure"/>
  <rdfs:range rdf:resource="#Item"/>
  <owl:inverseOf rdf:resource="#hasProcedure"/>
</owl:ObjectProperty>

<owl:ObjectProperty rdf:about="#has_image_for_step">
  <rdfs:domain rdf:resource="#Step"/>
  <rdfs:range rdf:resource="#Image"/>
